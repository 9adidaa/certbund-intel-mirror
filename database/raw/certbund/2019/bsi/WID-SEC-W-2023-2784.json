  "vulnerabilities": [
    {
      "cve": "CVE-2019-9543",
      "notes": [
        {
          "category": "description",
          "text": "In poppler existieren mehrere Schwachstellen. Die Schwachstellen befinden sich in JBIG2Stream::readGenericBitmap() und JBIG2Stream::readTextRegion() in JBIG2Stream.cc. Ein Angreifer kann dieses zu einem Denial of Service Angriff und möglicherweise zu weiteren Angriffen mit nicht spezifizierten Auswirkungen nutzen. Zur erfolgreichen Ausnutzung dieser Schwachstelle muss der Angreifer den Benutzer dazu bringen, eine speziell gestaltete PDF-Datei zu öffnen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T002207",
          "T013689"
        ]
      },
      "release_date": "2019-03-03T23:00:00.000+00:00",
      "title": "CVE-2019-9543"
    },
