  "vulnerabilities": [
    {
      "cve": "CVE-2019-12526",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Heap-Overflow Schwachstelle in Squid. Ein Angreifer kann dies zur Ausführung von beliebigem Code oder für einen Denial of Service Angriff ausnutzen."
        }
      ],
