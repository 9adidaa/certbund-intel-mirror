  "vulnerabilities": [
    {
      "cve": "CVE-2018-10851",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in PowerDNS. Vor dem Parsen wird Speicher allokiert und bei einem fehlerhaften Datensatz nicht mehr richtig freigegeben. Ein autorisierter Angreifer kann einen speziell gestalteten Datensatz in eine Zone die sich unter seiner Kontrolle befindet einfügen und dann eine DNS Abfrage für diesen Datensatz senden. In der Folge wird ein Speicherfehler verursacht, was zu einem Denial of Service führt."
        }
      ],
      "product_status": {
        "known_affected": [
          "T013060",
          "T002207",
          "T000126",
          "T013059"
        ]
      },
      "release_date": "2018-11-06T23:00:00.000+00:00",
      "title": "CVE-2018-10851"
    },
    {
      "cve": "CVE-2018-14626",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in PowerDNS. Ein entfernter anonymer Angreifer kann über eine speziell konstruierte DNS-Abfrage dafür sorgen, dass DNSSEC-validierende Clients Antworten als gefälscht betrachten, was zu einem Denial of Service führt."
        }
      ],
      "product_status": {
        "known_affected": [
          "T013060",
          "T002207",
          "T000126",
          "T013059"
        ]
      },
      "release_date": "2018-11-06T23:00:00.000+00:00",
      "title": "CVE-2018-14626"
    },
    {
      "cve": "CVE-2018-14644",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in PowerDNS. Ein entfernter anonymer Angreifer kann eine DNS-Anfrage für einen Meta-Typ wie OPT senden, was dazu führen kann, dass eine Zone fälschlicherweise als fehlerhafte DNSSEC-Validierung zwischengespeichert wird. Nachfolgende Anfragen von Clients, die eine DNSSEC-Validierung anfordern, werden mit einem ServFail beantwortet, was zu einem Denial of Service führt."
        }
      ],
      "product_status": {
        "known_affected": [
          "T013060",
          "T002207",
          "T000126",
          "T013059"
        ]
      },
      "release_date": "2018-11-06T23:00:00.000+00:00",
      "title": "CVE-2018-14644"
    }
  ]
