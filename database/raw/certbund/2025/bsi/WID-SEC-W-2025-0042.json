  "vulnerabilities": [
    {
      "cve": "CVE-2024-55224",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Vaultwarden. Der Fehler ermöglicht die Ausführung von benutzerdefiniertem HTML-Code durch Einfügen einer Nutzlast in das Feld \"Username\" einer E-Mail-Nachricht. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um HTML Code zur Ausführung zu bringen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "product_status": {
        "known_affected": [
          "T040113",
          "74185"
        ]
      },
      "release_date": "2025-01-09T23:00:00.000+00:00",
      "title": "CVE-2024-55224"
    },
    {
      "cve": "CVE-2024-55225",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Vaultwarden. Der Fehler befindet sich in der Komponente src/api/identity.rs und ermöglicht die Umgehung von Identifizierungsprozessen. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um andere User und sogar Administratoren nachahmen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T040113",
          "74185"
        ]
      },
      "release_date": "2025-01-09T23:00:00.000+00:00",
      "title": "CVE-2024-55225"
    },
    {
      "cve": "CVE-2024-55226",
      "notes": [
        {
          "category": "description",
          "text": "In Vaultwarden existiert eine Cross-Site Scripting Schwachstelle. HTML und Script-Eingaben werden in dem Modul /api/core/mod.rs nicht ordnungsgemäß überprüft, bevor sie an den Benutzer zurückgegeben werden. Ein entfernter, authentisierter Angreifer kann durch Ausnutzung dieser Schwachstelle beliebigen HTML- und Script-Code durch den Browser des Benutzers im Kontext der betroffenen Seite ausführen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "product_status": {
        "known_affected": [
          "T040113",
          "74185"
        ]
      },
      "release_date": "2025-01-09T23:00:00.000+00:00",
      "title": "CVE-2024-55226"
    }
