  "vulnerabilities": [
    {
      "cve": "CVE-2024-40747",
      "notes": [
        {
          "category": "description",
          "text": "In Joomla existiert eine Cross-Site Scripting Schwachstelle. HTML und Script-Eingaben werden in verschiedenen Module Chromes nicht ordnungsgemäß überprüft, bevor sie an den Benutzer zurückgegeben werden. Ein entfernter, anonymer Angreifer kann durch Ausnutzung dieser Schwachstelle beliebigen HTML- und Script-Code durch den Browser des Benutzers im Kontext der betroffenen Seite ausführen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "product_status": {
        "known_affected": [
          "1534555",
          "T040059"
        ]
      },
