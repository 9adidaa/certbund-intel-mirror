  "vulnerabilities": [
    {
      "cve": "CVE-2023-39963",
      "notes": [
        {
          "category": "description",
          "text": "In Nextcloud existieren mehrere Schwachstellen in den Komponenten Server, Enterprise, user_oidc, Notes und Talk Android. Ein Angreifer kann diese Schwachstellen ausnutzen, um einen Denial of Service zu verursachen, Sicherheitsvorkehrungen zu umgehen und Informationen offenzulegen. Zur erfolgreichen Ausnutzung einiger dieser Schwachstellen ist eine Anmeldung oder eine Interaktion des Nutzers notwendig."
        }
      ],
      "release_date": "2023-08-09T22:00:00.000+00:00",
      "title": "CVE-2023-39963"
    },
