  "vulnerabilities": [
    {
      "cve": "CVE-2022-39167",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in IBM SAN Volume Controller, IBM Storwize und IBM FlashSystem, wenn die Funktion \"IP Quorum\" in der IBM Spectum Virtualize Software aktiviert ist. Ein Angreifer in einer Man-in-the-Middle-Position kann bestimmte Konfigurationen ausnutzen, um vertrauliche Informationen offenzulegen."
        }
      ],
      "release_date": "2023-02-02T23:00:00.000+00:00",
      "title": "CVE-2022-39167"
    }
  ]
