  "vulnerabilities": [
    {
      "cve": "CVE-2023-26221",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in TIBCO Spotfire Analytics und TIBCO Spotfire. Dieser Fehler besteht in der Komponente Connectors, die es ermöglicht, bösartige Analyst-Dateien zu erstellen und damit Zugriffstoken von autorisierten Benutzern zu erhalten. Ein entfernter Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsvorkehrungen zu umgehen. Eine erfolgreiche Ausnutzung erfordert eine Benutzerinteraktion."
        }
      ],
      "release_date": "2023-11-08T23:00:00.000+00:00",
      "title": "CVE-2023-26221"
    }
