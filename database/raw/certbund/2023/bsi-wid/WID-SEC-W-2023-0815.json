  "vulnerabilities": [
    {
      "cve": "CVE-2023-28844",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Nextcloud. Der Fehler besteht, weil Benutzer, die nicht in der Lage sein sollten, eine Datei herunterzuladen, dennoch eine ältere Version der Datei herunterladen und diese für eine unkontrollierte Verteilung verwenden können. Ein entfernter, authentisierter Angreifer kann diese Schwachstelle ausnutzen, um vertrauliche Informationen offenzulegen. Eine erfolgreiche Ausnutzung erfordert eine Benutzerinteraktion."
        }
      ],
      "release_date": "2023-03-30T22:00:00.000+00:00",
      "title": "CVE-2023-28844"
    }
