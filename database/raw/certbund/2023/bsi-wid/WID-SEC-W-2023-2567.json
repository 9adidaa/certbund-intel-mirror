  "vulnerabilities": [
    {
      "cve": "CVE-2021-1572",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Cisco Network Services Orchestrator. Dieser ist auf einen Fehler hinsichtlich des SFTP-Service zurückzuführen, welcher mit erhöhten Privilegien ausgeführt wird. Ein lokaler Angreifer kann diese Schwachstelle ausnutzen, um beliebigen Programmcode mit Administratorrechten auszuführen."
        }
      ],
      "release_date": "2023-10-04T22:00:00.000+00:00",
      "title": "CVE-2021-1572"
    }
