  "vulnerabilities": [
    {
      "cve": "CVE-2023-3569",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Phoenix Contact TC ROUTER und TC CLOUD CLIENT Geräten bei der Verarbeitung von XML Inhalten. Ein authentisierter Angreifer kann dies ausnutzen, um mit einem speziell gestalteten XML Dokument einen Denial of Service Zustand herbeizuführen."
        }
      ],
      "release_date": "2023-08-07T22:00:00.000+00:00",
      "title": "CVE-2023-3569"
    },
    {
      "cve": "CVE-2023-3526",
      "notes": [
        {
          "category": "description",
          "text": "In Phoenix Contact TC ROUTER und TC CLOUD CLIENT Geräten existiert eine Cross-Site Scripting Schwachstelle. HTML und Script-Eingaben werden in der \"license viewer page\" nicht ordnungsgemäß überprüft, bevor sie an den Benutzer zurückgegeben werden. Ein entfernter, anonymer Angreifer kann durch Ausnutzung dieser Schwachstelle beliebigen HTML- und Script-Code durch den Browser des Benutzers im Kontext der betroffenen Seite ausführen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "release_date": "2023-08-07T22:00:00.000+00:00",
      "title": "CVE-2023-3526"
    }
  ]
