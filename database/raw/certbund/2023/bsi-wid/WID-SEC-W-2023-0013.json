  "vulnerabilities": [
    {
      "cve": "CVE-2022-45857",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Fortinet FortiManager. Der Fehler besteht aufgrund einer falschen Entscheidung in der Benutzerverwaltung in der VDOM-Erstellungskomponente. Ein Angreifer aus dem angrenzenden Netzwerk kann diese Schwachstelle ausnutzen, um über neu erstellte VDOMs ohne Passwort auf ein FortiGate zuzugreifen, nachdem das Admin-Konto \"super_admin\" gelöscht wurde. Eine erfolgreiche Ausnutzung erfordert eine Benutzerinteraktion."
        }
      ],
      "release_date": "2023-01-03T23:00:00.000+00:00",
      "title": "CVE-2022-45857"
    }
  ]
