  "vulnerabilities": [
    {
      "cve": "CVE-2022-42471",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Fortinet FortiWeb. Der Fehler besteht aufgrund einer unsachgemäßen Neutralisierung von CRLF-Sequenzen in HTTP-Headern, die als HTTP Response Splitting bekannt ist. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsmaßnahmen zu umgehen und beliebige Header einzufügen. Eine erfolgreiche Ausnutzung erfordert eine Benutzerinteraktion."
        }
      ],
      "release_date": "2023-01-03T23:00:00.000+00:00",
      "title": "CVE-2022-42471"
    }
  ]
