  "vulnerabilities": [
    {
      "cve": "CVE-2023-0026",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Juniper JUNOS. Diese besteht aufgrund einer unsachgemäßen Eingabevalidierung im Routing Protocol Daemon (rpd). Wird eine speziell gestaltete BGP-Update-Nachricht über eine aufgebaute BGP-Sitzung empfangen, dann kann diese Sitzung mit einem Fehler abgebrochen werden. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service zu verursachen."
        }
      ],
      "release_date": "2023-06-21T22:00:00.000+00:00",
      "title": "CVE-2023-0026"
    }
  ]
