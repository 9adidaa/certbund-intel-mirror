  "vulnerabilities": [
    {
      "cve": "CVE-2023-2514",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Mattermost. Während der Serverinitialisierung werden die Anmeldedaten der Datenbank in die Audit-Protokolle geschrieben. Ein Angreifer mit Zugriff auf die Audit-Protokolle kann dies ausnutzen, um diese Anmeldeinformationen offenzulegen."
        }
      ],
      "release_date": "2023-05-14T22:00:00.000+00:00",
      "title": "CVE-2023-2514"
    },
    {
      "cve": "CVE-2023-2515",
      "notes": [
        {
          "category": "description",
          "text": "Es existieret eine Schwachstelle im Mattermost Server bezüglich einer fehlerhaften Autorisierung. Ein authentisierter Angreifer mit der Berechtigung, andere Nutzerkonten zu bearbeiten und Personal Access Token auszustellen, kann seine Privilegien auf \"system admin\" erhöhen."
        }
      ],
      "release_date": "2023-04-12T22:00:00.000+00:00",
      "title": "CVE-2023-2515"
    }
  ]
