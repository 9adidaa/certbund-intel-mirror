  "vulnerabilities": [
    {
      "cve": "CVE-2023-20008",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Cisco TelePresence. Der Fehler besteht aufgrund unsachgemäßer Zugriffskontrollen auf Dateien, die sich im lokalen Dateisystem befinden. Ein lokaler Angreifer mit bestimmten Privilegien kann diese Schwachstelle ausnutzen, indem er einen symbolischen Link an einer bestimmten Stelle im lokalen Dateisystem eines betroffenen Geräts platziert, um beliebige Dateien zu überschreiben."
        }
      ],
      "product_status": {
        "known_affected": [
          "T025804",
          "T025805"
        ]
      },
      "release_date": "2023-01-11T23:00:00.000+00:00",
      "title": "CVE-2023-20008"
    },
    {
      "cve": "CVE-2023-20002",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Cisco TelePresence. Der Fehler besteht aufgrund einer unsachgemäßen Validierung der vom Benutzer bereitgestellten Eingaben, die zu einem SSRF führt. Ein lokaler Angreifer kann diese Schwachstelle ausnutzen, indem er eine manipulierte Anfrage an einen Benutzer der Webanwendung sendet, um beliebige Netzwerkanfragen zu senden, die von dem betroffenen System stammen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T025804",
          "T025805"
        ]
      },
      "release_date": "2023-01-11T23:00:00.000+00:00",
      "title": "CVE-2023-20002"
    }
  ]
