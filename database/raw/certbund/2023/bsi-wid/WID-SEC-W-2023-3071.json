  "vulnerabilities": [
    {
      "cve": "CVE-2023-31582",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Red Hat JBoss Data Grid. Es ist möglich, fälschlicherweise eine niedrige Iterationszahl von 1000 oder weniger einzustellen, um das Json-Web-Token zu sichern. Ein authentisierter Angreifer kann diese Schwachstelle ausnutzen, um Informationen offenzulegen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T016960"
        ]
      },
      "release_date": "2023-12-06T23:00:00.000+00:00",
      "title": "CVE-2023-31582"
    },
    {
      "cve": "CVE-2023-4586",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Red Hat JBoss Data Grid. Der Hot Rod-Client aktiviert bei der Verwendung von TLS keine Hostnamen-Validierung. Ein Angreifer kann diese Schwachstelle ausnutzen, um einen Man in the Middle Angriff durchzuführen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T016960"
        ]
      },
      "release_date": "2023-12-06T23:00:00.000+00:00",
      "title": "CVE-2023-4586"
    },
    {
      "cve": "CVE-2023-5384",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Red Hat JBoss Data Grid. In Infinispan werden bei der Serialisierung der Konfiguration für einen Cache in XML/JSON/YAML, die Anmeldeinformationen im Klartext als Teil der Konfiguration zurückgegeben. Ein Angreifer kann diese Schwachstelle ausnutzen, um Informationen offenzulegen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T016960"
        ]
      },
      "release_date": "2023-12-06T23:00:00.000+00:00",
      "title": "CVE-2023-5384"
    }
  ]
