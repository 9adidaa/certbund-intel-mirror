  "vulnerabilities": [
    {
      "cve": "CVE-2023-25950",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in HAProxy Enterprise. Ein fehlerhafter HTTP-Header-Feldnamen wird in der HTTP/3-Implementierung nicht sachgemäß blockiert, was zu einem HTTP-Request/Response-Schmuggelangriff genutzt werden kann. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um Informationen offenzulegen, oder einen Denial of Service zu verursachen."
        }
      ],
      "release_date": "2023-03-30T22:00:00.000+00:00",
      "title": "CVE-2023-25950"
    }
