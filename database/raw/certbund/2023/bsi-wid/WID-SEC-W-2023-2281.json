  "vulnerabilities": [
    {
      "cve": "CVE-2023-4809",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in FreeBSD Project FreeBSD OS. In der pf-Paketverarbeitung werden IPv6 Pakete, die mehrere IPv6-Fragment-Header enthält unsachgemäß verarbeitet, wodurch sie von bestimmten Firewall Regeln nicht sachgemäß verarbeitet werden können. Ein Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsvorkehrungen zu umgehen."
        }
      ],
      "release_date": "2023-09-06T22:00:00.000+00:00",
      "title": "CVE-2023-4809"
    }
  ]
