  "vulnerabilities": [
    {
      "cve": "CVE-2022-43946",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Fortinet FortiClient. Der Fehler besteht aufgrund einer falschen Rechtevergabe für kritische Ressourcen und einer Time-of-Check Time-of-Use Race Condition. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um seine Privilegien zu erweitern. Eine erfolgreiche Ausnutzung erfordert eine Benutzerinteraktion."
        }
      ],
      "release_date": "2023-04-11T22:00:00.000+00:00",
      "title": "CVE-2022-43946"
    },
    {
      "cve": "CVE-2022-42470",
      "notes": [
        {
          "category": "description",
          "text": "In Fortinet FortiClient Windows existieren mehrere Schwachstellen. Die Fehler bestehen aufgrund eines Pfad-Traversals und einer falschen Autorisierung. Ein lokaler Angreifer kann diese Schwachstellen ausnutzen, um beliebige Dateien auf dem Gerätedateisystem zu erstellen."
        }
      ],
      "release_date": "2023-04-11T22:00:00.000+00:00",
      "title": "CVE-2022-42470"
    },
