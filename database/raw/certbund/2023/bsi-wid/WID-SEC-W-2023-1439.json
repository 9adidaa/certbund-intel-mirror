  "vulnerabilities": [
    {
      "cve": "CVE-2023-25609",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Fortinet FortiAnalyzer und Fortinet FortiManager. Der Fehler besteht aufgrund einer serverseitigen Anforderungsfälschung in der GUI. Ein entfernter, authentisierter Angreifer kann diese Schwachstelle ausnutzen, um über speziell gestaltete Webanfragen auf nicht autorisierte Dateien und Dienste auf dem System zuzugreifen."
        }
      ],
      "release_date": "2023-06-12T22:00:00.000+00:00",
      "title": "CVE-2023-25609"
    }
