  "vulnerabilities": [
    {
      "cve": "CVE-2023-33307",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Fortinet FortiOS und Fortinet FortiProxy. Der Fehler besteht aufgrund einer NULL-Zeiger-Dereferenz in SSL-VPN. Ein entfernter, authentisierter Angreifer kann diese Schwachstelle ausnutzen, indem er manipulierte Anfragen verwendet, um einen Denial-of-Service-Zustand auszulösen."
        }
      ],
      "release_date": "2023-06-18T22:00:00.000+00:00",
