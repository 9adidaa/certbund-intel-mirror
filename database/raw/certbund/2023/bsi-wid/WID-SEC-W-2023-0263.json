  "vulnerabilities": [
    {
      "cve": "CVE-2022-23498",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Grafana. Wenn das Zwischenspeichern von Datenquellenabfragen aktiviert ist, speichert Grafana alle Header, einschließlich der Rotation des Grafana-Session-Cookies über einen Set-Cookie grafana_session-Header. Ein authentisierter Angreifer kann diese Schwachstelle ausnutzen, um das gecachte Session-Cookie eines anderen Benutzers zu erhalten."
        }
      ],
