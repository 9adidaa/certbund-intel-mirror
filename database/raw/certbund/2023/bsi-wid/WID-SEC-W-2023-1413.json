  "vulnerabilities": [
    {
      "cve": "CVE-2023-35036",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Progress Software MOVEit. Dabei handelt es sich um eine SQL-Injection in der MOVEit Transfer-Webanwendung, die einen unbefugten Zugriff auf die MOVEit Transfer-Datenbank ermöglicht. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um eine manipulierte Payload an einen Endpunkt der MOVEit Transfer-Anwendung zu senden, was zur Änderung und Offenlegung von MOVEit-Datenbankinhalten führt."
        }
      ],
      "release_date": "2023-06-11T22:00:00.000+00:00",
      "title": "CVE-2023-35036"
    }
  ]
