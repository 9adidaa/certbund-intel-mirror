  "vulnerabilities": [
    {
      "cve": "CVE-2023-1831",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Mattermost. Das Benutzerpasswort bei der Benutzererstellung und der Benutzerpasswort-Hash bei anderen Vorgängen wird nicht aus den Audit-Protokollen entfernt, wenn die experimentelle Audit-Protokollierungskonfiguration aktiviert ist. Ein privilegierter Angreifer kann diese Schwachstelle ausnutzen, um Informationen offenzulegen."
        }
      ],
      "release_date": "2023-03-19T23:00:00.000+00:00",
      "title": "CVE-2023-1831"
    }
