  "vulnerabilities": [
    {
      "cve": "CVE-2023-6178",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Tenable Security Nessus. Der Fehler besteht aufgrund eines willkürlichen Dateischreibens. Ein entfernter, authentisierter Angreifer mit Privilegien des Dienstes kann diese Schwachstelle ausnutzen, indem er Variablen aus Nessus-Regeln ändert, um einen Denial-of-Service-Zustand auszulösen."
        }
      ],
      "release_date": "2023-11-16T23:00:00.000+00:00",
      "title": "CVE-2023-6178"
    },
