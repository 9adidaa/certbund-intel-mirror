  "vulnerabilities": [
    {
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in MediaWiki. Diese ist auf einen Fehler in der Komponente \"OATHAuth\" zurückzuführen, durch die einen Anfälligkeit für Replay-Angriffe besteht. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsvorkehrungen zu umgehen."
        }
      ],
      "release_date": "2023-03-30T22:00:00.000+00:00"
    },
    {
      "cve": "CVE-2020-36649",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert mehrere Schwachstellen in MediaWiki. Diese sind auf einen unzureichenden Schutz gegen Brute-Force-Angriffe sowie einem Fehler in der Komponente \"PapaParse\" zurückzuführen. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service Zustand herbeizuführen."
        }
      ],
      "release_date": "2023-03-30T22:00:00.000+00:00",
      "title": "CVE-2020-36649"
    }
  ]
