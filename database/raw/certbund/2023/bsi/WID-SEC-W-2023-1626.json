  "vulnerabilities": [
    {
      "cve": "CVE-2023-29197",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in MediaWiki, die zum aktuellen Zeitpunkt nicht im Detail beschrieben und veröffentlicht wurde. Der Fehler besteht in der Komponente guzzlehttp/psr7. Ein Angreifer kann diese Schwachstelle ausnutzen, um nicht näher spezifizierte Auswirkungen zu verursachen."
        }
      ],
      "product_status": {
