  "vulnerabilities": [
    {
      "cve": "CVE-2022-48538",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Cacti. Diese Schwachstelle besteht in der Web-Login-Funktionalität, wo \"cacti_ldap_auth()\" eine Null als Passwort erlaubt. Ein Angreifer kann diese Schwachstelle ausnutzen, um die Authentifizierung zu umgehen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T029453",
          "T012167"
        ]
      },
      "release_date": "2023-08-22T22:00:00.000+00:00",
      "title": "CVE-2022-48538"
    },
    {
      "cve": "CVE-2022-48547",
      "notes": [
        {
          "category": "description",
          "text": "In Cacti existiert eine Cross-Site Scripting Schwachstelle. HTML und Script-Eingaben werden im \"ref\" Parameter von \"auth_changepassword.php\" nicht ordnungsgemäß überprüft, bevor sie an den Benutzer zurückgegeben werden. Ein entfernter, anonymer Angreifer kann durch Ausnutzung dieser Schwachstelle beliebigen HTML- und Script-Code durch den Browser des Benutzers im Kontext der betroffenen Seite ausführen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T012167"
        ],
        "last_affected": [
          "T029451"
        ]
      },
      "release_date": "2023-08-22T22:00:00.000+00:00",
      "title": "CVE-2022-48547"
    }
