  "vulnerabilities": [
    {
      "cve": "CVE-2023-0105",
      "notes": [
        {
          "category": "description",
          "text": "Es gibt eine Schwachstelle in Keycloak aufgrund unsachgemäßer Handhabung von E-Mail-Vertrauenseinstellungen. Der Status \"verifiziert\" wird nicht zurückgesetzt, wenn sich die Email-Adresse ändert. Ein Angreifer kann dies ausnutzen, um andere Benutzer mit der gleichen E-Mail-Adresse zu beschatten, auszusperren oder sich für sie auszugeben."
        }
      ],
