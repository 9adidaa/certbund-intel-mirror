  "vulnerabilities": [
    {
      "cve": "CVE-2023-36851",
      "notes": [
        {
          "category": "description",
          "text": "In Juniper JUNOS auf Geräten der EX- und SRX-Serie bestehen mehrere Schwachstellen in der Komponente J-WEB. Sie sind auf eine fehlende Authentifizierung und eine externe PHP-Variablenmodifikation zurückzuführen. Ein entfernter, anonymer Angreifer kann diese Schwachstellen in Kombination ausnutzen, um beliebigen Code auszuführen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T019811",
          "T021593"
        ]
      },
      "release_date": "2023-08-17T22:00:00.000+00:00",
      "title": "CVE-2023-36851"
