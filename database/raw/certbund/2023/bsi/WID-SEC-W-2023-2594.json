  "vulnerabilities": [
    {
      "cve": "CVE-2023-45374",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in MediaWiki. Dieser Fehler besteht in der SportsTeams-Erweiterung, da diese nicht richtig auf das Anti-CSRF-Edit-Token prüft. Ein entfernter Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsmaßnahmen zu umgehen."
        }
      ],
      "product_status": {
        "known_affected": [
          "2951"
        ]
      },
      "release_date": "2023-10-08T22:00:00.000+00:00",
      "title": "CVE-2023-45374"
    },
    {
      "cve": "CVE-2023-45373",
      "notes": [
        {
          "category": "description",
          "text": "In MediaWiki existiert eine Cross-Site Scripting Schwachstelle. HTML und Script-Eingaben werden in ProofreadPage nicht ordnungsgemäß überprüft, bevor sie an den Benutzer zurückgegeben werden. Ein entfernter, anonymer Angreifer kann durch Ausnutzung dieser Schwachstelle beliebigen HTML- und Script-Code durch den Browser des Benutzers im Kontext der betroffenen Seite ausführen."
        }
      ],
      "product_status": {
        "known_affected": [
          "2951"
        ]
      },
      "release_date": "2023-10-08T22:00:00.000+00:00",
      "title": "CVE-2023-45373"
    },
    {
      "cve": "CVE-2023-45372",
      "notes": [
        {
          "category": "description",
          "text": "Es bestehen mehrere Schwachstellen in MediaWiki. Diese Fehler bestehen in der Wikibase-Erweiterung, da keine Begrenzung für das Zusammenführen von Einträgen besteht und ItemMergeInteractor keinen Bearbeitungsfilter während des Zusammenführens von Einträgen laufen lässt. Ein Angreifer kann diese Schwachstelle ausnutzen, um Dateien zu manipulieren."
        }
      ],
      "product_status": {
        "known_affected": [
          "2951"
        ]
      },
      "release_date": "2023-10-08T22:00:00.000+00:00",
      "title": "CVE-2023-45372"
    },
