  "vulnerabilities": [
    {
      "cve": "CVE-2022-45143",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Apache Tomcat aufgrund eines fehlenden \"Escaping\" bestimmter vom Benutzer eingegebenen Werte in \"JsonErrorReportValve\". Ein Angreifer kann dies ausnutzen, um die resultierenden JSON-Daten zu manipulieren."
        }
      ],
