  "vulnerabilities": [
    {
      "cve": "CVE-2023-26115",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Red Hat OpenShift. Diese ist auf eine Anfälligkeit für einen Regular Expression Denial of Service in der Komponente \"Node.js word-wrap\" zurückzuführen. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service Zustand herbeizuführen."
        }
      ],
