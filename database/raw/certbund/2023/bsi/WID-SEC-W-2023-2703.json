  "vulnerabilities": [
    {
      "cve": "CVE-2023-5552",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Sophos Firewall. Es besteht ein Fehler bei der Verschlüsselungsmethode \"specified by Sender\", durch die das gewählte Passwort zur Verschlüsselung offengelegt werden kann. Ein entfernter, authentisierter Angreifer kann diese Schwachstelle ausnutzen, um Informationen offenzulegen."
        }
      ],
      "release_date": "2023-10-17T22:00:00.000+00:00",
      "title": "CVE-2023-5552"
    }
