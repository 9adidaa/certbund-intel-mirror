  "vulnerabilities": [
    {
      "cve": "CVE-2023-22995",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle im Linux Kernel. In dwc3_qcom_acpi_register_core in drivers/usb/dwc3/dwc3-qcom.c fehlen bestimmte platform_device_put- und kfree-Aufrufe. Ein Angreifer kann diese Schwachstelle ausnutzen, um nicht näher spezifizierte Auswirkungen zu verursachen."
        }
      ],
