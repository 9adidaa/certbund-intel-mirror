  "vulnerabilities": [
    {
      "cve": "CVE-2023-5541",
      "notes": [
        {
          "category": "description",
          "text": "In Moodle existiert eine Cross-Site Scripting Schwachstelle. HTML und Script-Eingaben werden in beim Import eines Arbeitsblattes nicht ordnungsgemäß überprüft, bevor sie an den Benutzer zurückgegeben werden. Ein entfernter, anonymer Angreifer kann durch Ausnutzung dieser Schwachstelle beliebigen HTML- und Script-Code durch den Browser des Benutzers im Kontext der betroffenen Seite ausführen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "release_date": "2023-10-16T22:00:00.000+00:00",
      "title": "CVE-2023-5541"
    },
    {
      "cve": "CVE-2023-5540",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Moodle. Diese besteht während der IMSCP-Aktivität. Ein Angreifer kann diese Schwachstelle ausnutzen, um Code auszuführen."
        }
      ],
      "release_date": "2023-10-16T22:00:00.000+00:00",
      "title": "CVE-2023-5540"
    },
    {
      "cve": "CVE-2023-5539",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Moodle. Diese besteht während des Unterrichts und kann nur von angemeldeten Lehrern oder Managern ausgenutzt werden. Ein Angreifer kann diese Schwachstelle ausnutzen, um Code auszuführen."
        }
      ],
      "release_date": "2023-10-16T22:00:00.000+00:00",
      "title": "CVE-2023-5539"
    }
