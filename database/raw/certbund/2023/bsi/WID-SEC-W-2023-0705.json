  "vulnerabilities": [
    {
      "cve": "CVE-2023-25820",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Nextcloud. Der \"Confirmation Endpoint\" verfügt nicht über einen Brute-Force-Schutz. Ein Angreifer, der Zugriff auf eine bereits eingeloggte Benutzersitzung erhält, kann dies ausnutzen, um einen Brute-Force-Angriff auf das Passwort des betroffenen Benutzerkontos durchzuführen."
        }
      ],
      "release_date": "2023-03-21T23:00:00.000+00:00",
      "title": "CVE-2023-25820"
    }
