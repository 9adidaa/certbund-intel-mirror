  "vulnerabilities": [
    {
      "cve": "CVE-2023-2985",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle im Linux Kernel. In \"hfsplus_put_super\" in \"fs/hfsplus/super.c\" besteht ein use-after-free Problem. Ein lokaler Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service zu verursachen."
        }
      ],
