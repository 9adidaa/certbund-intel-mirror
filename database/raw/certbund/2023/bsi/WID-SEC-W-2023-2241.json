  "vulnerabilities": [
    {
      "cve": "CVE-2023-20900",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in VMware Tools. Diese besteht aufgrund eines Problems in der Überprüfung der SAML-Tokensignatur. Ein Angreifer in einer Man-in-the-Middle (MITM)-Netzwerkpositionierung zwischen dem vCenter-Server und der virtuellen Maschine kann diese Schwachstelle ausnutzen, um die SAML-Tokensignaturprüfung zu umgehen."
        }
      ],
