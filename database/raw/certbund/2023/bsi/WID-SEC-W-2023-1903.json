  "vulnerabilities": [
    {
      "cve": "CVE-2023-32629",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle im Ubuntu Linux Kernel. Der Fehler besteht aufgrund einer unsachgemäßen Berechtigungsprüfung in overlayfs ovl_copy_up_meta_inode_data beim Aufruf von ovl_do_setxattr. Ein lokaler Angreifer kann diese Schwachstelle ausnutzen, um seine Privilegien zu erweitern."
        }
      ],
      "product_status": {
        "known_affected": [
          "T000126",
          "T028935"
        ]
      },
      "release_date": "2023-07-26T22:00:00.000+00:00",
      "title": "CVE-2023-32629"
    },
    {
      "cve": "CVE-2023-2640",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle im Ubuntu Linux Kernel. Der Fehler besteht aufgrund einer unsachgemäßen Berechtigungsprüfung für trusted.overlayfs.* xattrs\". Ein lokaler Angreifer kann diese Schwachstelle ausnutzen, um privilegierte erweiterte Attribute auf die eingehängten Dateien zu setzen und so die Sicherheitsmaßnahmen zu umgehen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T000126",
          "T028935"
        ]
      },
      "release_date": "2023-07-26T22:00:00.000+00:00",
      "title": "CVE-2023-2640"
    }
  ]
