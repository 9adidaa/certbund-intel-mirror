  "vulnerabilities": [
    {
      "cve": "CVE-2023-1101",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in SonicWall SonicOS in der \"SSLVPN\" Komponente. Aufgrund von fehlenden Restriktionen von Versuchen bei der Multifaktorauthentisierung kann eine große Anzahl an MFA-Codes verbraucht werden. Ein entfernter, anonymer Angreifer kann dies ausnutzen, um unbekannte Auswirkungen zu verursachen."
        }
      ],
      "release_date": "2023-03-02T23:00:00.000+00:00",
      "title": "CVE-2023-1101"
    },
    {
      "cve": "CVE-2023-0656",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in SonicWall SonicOS bezüglich der \"web management\" Schnittstelle. Diese ist auf einen stapelbasierten Pufferüberlauf zurückzuführen. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service Zustand herbeizuführen."
        }
      ],
      "release_date": "2023-03-02T23:00:00.000+00:00",
      "title": "CVE-2023-0656"
    }
  ]
