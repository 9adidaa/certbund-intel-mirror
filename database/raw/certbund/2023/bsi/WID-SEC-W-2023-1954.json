  "vulnerabilities": [
    {
      "cve": "CVE-2023-3470",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in bestimmten F5 BIG-IP Plattformen mit Cavium Nitrox FIPS HSM-Karten. Diese generieren ein deterministisches Passwort für das Crypto User-Konto. Die vorhersehbare Natur des Kennworts ermöglicht einem authentifizierten Angreifer mit TMOS Shell (tmsh) Zugriff auf das BIG-IP-System oder jedem, der physischen Zugriff auf das FIPS HSM hat, die Informationen, die zur Generierung des richtigen Kennworts erforderlich sind. Ein privilegierter oder physischer Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsvorkehrungen zu umgehen."
        }
      ],
      "release_date": "2023-08-02T22:00:00.000+00:00",
      "title": "CVE-2023-3470"
    }
  ]
