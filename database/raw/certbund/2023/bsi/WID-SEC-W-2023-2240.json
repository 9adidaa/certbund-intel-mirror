  "vulnerabilities": [
    {
      "cve": "CVE-2023-38408",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in IBM AIX und IBM VIOS. Dieser Fehler existiert in OpenSSH aufgrund eines Fehlers im weitergeleiteten ssh-agent. Durch das Senden speziell gestalteter Anfragen kann ein entfernter Angreifer diese Schwachstelle zur Ausführung von beliebigem Code ausnutzen."
        }
      ],
      "release_date": "2023-08-30T22:00:00.000+00:00",
      "title": "CVE-2023-38408"
    },
    {
      "cve": "CVE-2023-40371",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in IBM AIX und IBM VIOS. Dieser Fehler besteht in der OpenSSH-Implementierung aufgrund einer ungeeigneten Zugriffskontrolle, die den Zugriff auf Dateien ermöglicht. Ein lokaler Angreifer kann diese Schwachstelle ausnutzen, um vertrauliche Informationen offenzulegen."
        }
      ],
      "release_date": "2023-08-30T22:00:00.000+00:00",
      "title": "CVE-2023-40371"
    }
  ]
