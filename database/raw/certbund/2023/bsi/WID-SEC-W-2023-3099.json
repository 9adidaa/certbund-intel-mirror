  "vulnerabilities": [
    {
      "cve": "CVE-2023-5129",
      "notes": [
        {
          "category": "description",
          "text": "In Unify OpenScape Produkten existieren mehrere Schwachstellen. In Google WebP (libwebp) besteht der Funktion BuildHuffmanTable() in utils/huffman_utils.c, eine Überlaufbedingung, die beim Dekodieren bestimmter Streams ausgelöst wird. Ein Angreifer kann diese Schwachstelle ausnutzen, um beliebigen Code zur Ausführung zu bringen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "product_status": {
        "known_affected": [
          "T031599"
        ]
      },
      "release_date": "2023-12-11T23:00:00.000+00:00",
      "title": "CVE-2023-5129"
