    {
      "cve": "CVE-2023-20047",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Cisco WebEx. Der Fehler besteht in den Komponenten Room Phone und Share Link Layer Discovery Protocol aufgrund einer unzureichenden Ressourcenzuweisung. Ein Angreifer aus dem angrenzenden Netzwerk kann diese Schwachstelle ausnutzen, indem er manipulierten LLDP-Datenverkehr an ein betroffenes Gerät sendet und so vertrauliche Informationen offenlegt."
        }
      ],
      "release_date": "2023-01-11T23:00:00.000+00:00",
      "title": "CVE-2023-20047"
    }
  ]
