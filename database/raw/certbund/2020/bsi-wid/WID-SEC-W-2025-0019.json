  "vulnerabilities": [
    {
      "cve": "CVE-2020-14299",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in der Red Hat JBoss Enterprise Application Platform. Das erneute Laden von JBoss EAP in den Nur-Administrator-Modus ermöglicht die Umgehung der Authentifizierung. Ein authentisierter Angreifer kann diese Schwachstelle nutzen, um einen Denial of Service zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T040040",
          "67646",
          "T017492"
        ]
      },
      "release_date": "2020-10-13T22:00:00.000+00:00",
      "title": "CVE-2020-14299"
    },
    {
      "cve": "CVE-2020-14338",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in der Red Hat JBoss Enterprise Application Platform. Aufgrund einer unsachgemäßen Eingabevalidierung ist eine Manipulation der XML-Validierung aufgrund unvollständiger Anwendung von use-grammar-pool-only in xercesImpl möglich. Ein entfernter anonymer Angreifer kann diese Schwachstelle ausnutzen, um die Integrität des Dienstes zu gefährden."
        }
      ],
      "product_status": {
        "known_affected": [
          "T040040",
          "67646",
          "T017492"
        ]
      },
      "release_date": "2020-10-13T22:00:00.000+00:00",
      "title": "CVE-2020-14338"
    },
    {
      "cve": "CVE-2020-14340",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in der Red Hat JBoss Enterprise Application Platform. Eine hohe Anzahl von NIO Selector-Dateihandles kann ein Dateideskriptorleck verursachen. Ein entfernter anonymer Angreifer kann das ausnutzen, um einen Denial of Service zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
