  "vulnerabilities": [
    {
      "cve": "CVE-2020-11945",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Squid. Diese besteht in der HTTP Digest Authentication. Aufgrund eines Integer Überlaufs kann ein entfernter anonymer Angreifer über HTTP Digest Authentication Token einen Credential Replay Angriff durchführen oder beliebigen Code zur Ausführung bringen."
        }
      ],
