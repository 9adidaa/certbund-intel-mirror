  "vulnerabilities": [
    {
      "cve": "CVE-2020-16135",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in libssh. Diese beruht auf einer Null-Zeiger Dereferenzierung in tftpserver.c if ssh_buffer_new. Ein entfernter anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service zu verursachen."
        }
      ],
