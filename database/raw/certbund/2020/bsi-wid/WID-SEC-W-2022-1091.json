  "vulnerabilities": [
    {
      "cve": "CVE-2020-15863",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in QEMU, welche in der \"XGMAC NIC\" Komponente besteht. Es handelt sich dabei um einen Buffer-Overflow, welcher bei der Verarbeitung von Paketen auftritt. Ein lokaler Angreifer im Gastsystem kann diese Schwachstelle ausnutzen, um einen Denial of Service Zustand auf dem Host auszulösen oder unter Umständen beliebigen Programmcode auszuführen."
        }
      ],
