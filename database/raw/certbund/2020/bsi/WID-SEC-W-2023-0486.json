  "vulnerabilities": [
    {
      "cve": "CVE-2020-8201",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Node.js. Carriage Returns werden in HTTP-Request-Headern vor dem Parsen in einen Bindestrich konvertiert. Dies kann zum Schmuggeln von HTTP-Anfragen führen, da es sich um eine nicht standardmäßige Interpretation des Headers handelt. Ein Angreifer kann das ausnutzen, um Sicherheitsvorkehrungen zu umgehen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T002207",
          "67646",
          "T000126",
          "T026522",
          "T012167",
          "T004914",
          "T017494"
        ]
      },
      "release_date": "2020-09-15T22:00:00.000+00:00",
      "title": "CVE-2020-8201"
    },
    {
      "cve": "CVE-2020-8251",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Node.js. Aufgrund einer verzögerten Übermittlung von Anfragen ist ein Server unter Umständen nicht in der Lage neue Verbindungen anzunehmen. Ein entfernter anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T002207",
          "67646",
          "T000126",
          "T026522",
          "T012167",
          "T004914"
        ]
      },
      "release_date": "2020-09-15T22:00:00.000+00:00",
      "title": "CVE-2020-8251"
    },
    {
      "cve": "CVE-2020-8252",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Node.js. Aufgrund einer falschen Bestimmung der Puffergröße in der Realpath-Implementierung von libuv kommt es zu einem Puffer Überlauf, wenn der aufgelöste Pfad länger als 256 Bytes ist. Ein Angreifer kann diese Schwachstelle mit nicht spezifizierten Auswirkungen ausnutzen."
        }
      ],
      "product_status": {
