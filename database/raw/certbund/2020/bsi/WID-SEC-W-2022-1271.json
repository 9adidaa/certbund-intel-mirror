  "vulnerabilities": [
    {
      "cve": "CVE-2020-25624",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in QEMU. Diese besteht aufgrund eines \"stack-based buffer over-read\" Fehlers in [hw/usb/hcd-ohci.c]. Ein Angreifer kann diese Schwachstelle ausnutzen, um Informationen offenzulegen."
        }
      ],
