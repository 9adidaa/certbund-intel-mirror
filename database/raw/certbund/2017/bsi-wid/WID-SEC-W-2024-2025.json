  "vulnerabilities": [
    {
      "cve": "CVE-2017-5578",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in QEMU, wenn der QEMU Emulator mit Virtio GPU Device Emulator Unterstützung kompiliert wurde. Die Schwachstelle besteht in der \"virtio_gpu_resource_attach_backing\" Funktion in [hw/display/virtio-gpu.c]. Ein Angreifer aus einem Gast System kann durch Übermitteln geeignet gestalteter Daten diese Schwachstelle zu einem Denial of Serivce Angriff ausnutzen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T002207",
          "T000126",
          "T007150",
          "T004914"
        ]
      },
      "release_date": "2017-04-25T22:00:00.000+00:00",
      "title": "CVE-2017-5578"
    },
    {
      "cve": "CVE-2017-8086",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in QEMU, wenn der QEMU Emulator mit virtio-9p back-end Unterstützung kompiliert wurde. Die Schwachstelle tritt bei der Verarbeitung von Daten in der \"9pfs_list_xattr()\" Routine auf. Ein Angreifer aus einem Gast System kann diese Schwachstelle durch Übermitteln geeigent gestalteter Daten zu einem Denial of Service Angriff ausnutzen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T002207",
          "T000126",
          "T007150",
          "T004914"
        ]
      },
      "release_date": "2017-04-25T22:00:00.000+00:00",
      "title": "CVE-2017-8086"
    },
    {
      "cve": "CVE-2017-8112",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in QEMU, wenn der QEMU Emulator mit VMWARE PVSCSI paravirtual SCSI bus Unterstützung kompiliert wurde. Die Schwachstelle tritt bei der Initialisierung von Ring Puffern für SCSI Nachrichten in der Funktion \"pvscsi_ring_init_msg()\" auf. Ein Angreifer aus einem Gast System kann diese Schwachstelle durch Übermitteln geeigent gestalteter Daten zu einem Denial of Service Angriff ausnutzen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T002207",
          "T000126",
          "T007150",
          "T004914"
        ]
      },
      "release_date": "2017-04-25T22:00:00.000+00:00",
      "title": "CVE-2017-8112"
    }
  ]
