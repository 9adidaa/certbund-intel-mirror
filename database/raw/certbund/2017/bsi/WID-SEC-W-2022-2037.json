  "vulnerabilities": [
    {
      "cve": "CVE-2017-9233",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in expat. Ein Angreifer kann ein spezielles XML File erstellen, das bei Verarbeitung zu einem Denial of Service führt. Zur erfolgreichen Ausnutzung dieser Schwachstelle muss der Angreifer den Benutzer dazu bringen eine modifizierte URL oder Webseite in seinem Web-Browser zu öffnen."
        }
      ],
