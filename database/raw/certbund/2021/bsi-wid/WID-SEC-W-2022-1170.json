  "vulnerabilities": [
    {
      "cve": "CVE-2021-3326",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in GNU libc, welche aufgrund eines \"Reachable Assertion\" besteht. Die Schwachstelle tritt bei der Konvertierung von ISO-2022-JP-3 zum UTF-8 Format auf. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service Zustand herbeizuführen."
        }
      ],
