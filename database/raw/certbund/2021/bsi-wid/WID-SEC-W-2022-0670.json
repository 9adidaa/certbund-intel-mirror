  "vulnerabilities": [
    {
      "cve": "CVE-2020-25097",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Squid. Die Schwachstelle besteht aufgrund einer unsachgemäßen Eingabevalidierung und ermöglicht 'HTTP-Request-Smuggling'. Ein entfernter anonymer Angreifer kann diese Sicherheitslücke ausnutzen, um Sicherheitsmaßnahmen zu umgehen."
        }
      ],
