  "vulnerabilities": [
    {
      "cve": "CVE-2021-20313",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in ImageMagick. Der Fehler besteht in \"TransformSignature\" in \"MagickCore/signature.c\" aufgrund eines potenziellen Cipher-Lecks. Ein Angreifer kann diese Schwachstelle ausnutzen, um sensible Informationen offenzulegen."
        }
      ],
