  "vulnerabilities": [
    {
      "cve": "CVE-2022-27518",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine nicht näher beschriebene Schwachstelle in Citrix Systems ADC und Citrix Systems Citrix Gateway. Diese besteht, wenn ADC bzw. Gateway als \"SAML SP\" oder \"SAML IdP\" konfiguriert sind. Ein entfernter, anonymer Angreifer kann dadurch beliebigen Code auf der betroffenen Appliance ausführen."
        }
      ],
