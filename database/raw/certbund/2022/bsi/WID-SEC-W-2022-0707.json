  "vulnerabilities": [
    {
      "cve": "CVE-2021-38561",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Red Hat OpenShift in der Komponente Logging Subsystem. Der Fehler besteht aufgrund eines Out-of-Bounds-Read in golang.org/x/text/language. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial-of-Service-Zustand zu verursachen."
        }
      ],
