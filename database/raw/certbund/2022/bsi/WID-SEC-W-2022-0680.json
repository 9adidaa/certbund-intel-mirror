  "vulnerabilities": [
    {
      "cve": "CVE-2022-22473",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in IBM WebSphere Application Server aufgrund unsachgemäßer Handhabung von Daten der Verwaltungskonsole. Ein Angreifer kann dies ausnutzen, um vertrauliche Informationen zu erhalten, die für weitere Angriffe auf das System genutzt werden können."
        }
      ],
