  "vulnerabilities": [
    {
      "cve": "CVE-2022-35843",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Fortinet FortiOS und Fortinet FortiProxy. In der SSH-Anmeldekomponente besteht ein Fehler durch \"assumed immutable\" Daten. Ein entfernter anonymer Angreifer kann diese Schwachstelle ausnutzen, um sich in das Gerät einzuloggen, indem er eine speziell gestaltete Access-Challenge-Antwort vom Radius-Server sendet."
        }
      ],
      "release_date": "2022-12-06T23:00:00.000+00:00",
      "title": "CVE-2022-35843"
    }
  ]
