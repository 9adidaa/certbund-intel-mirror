  "vulnerabilities": [
    {
      "cve": "CVE-2022-46836",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in tribe29 checkmk. Der Fehler besteht aufgrund einer unsachgemäßen Bereinigung der in Wato eingegebenen Daten beim Schreiben in eine PHP-Datei. Ein entfernter, authentisierter Angreifer kann diese Schwachstelle ausnutzen, indem er PHP-Code in Dateien einschleust, die von Wato für die NagVis-Integration erzeugt wurden, um beliebigen Code auszuführen."
        }
      ],
      "release_date": "2022-09-01T22:00:00.000+00:00",
      "title": "CVE-2022-46836"
    },
    {
      "cve": "CVE-2022-46303",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in tribe29 checkmk. Der Fehler besteht aufgrund eines unsachgemäßen Escape des SMS-Skripts beim Versand von SMS-Benachrichtigungen mit eingefügten beliebigen Shell-Befehlen. Ein Angreifer aus dem angrenzenden Netzwerk kann diese Schwachstelle ausnutzen, indem er eine manipulierte Zeichenfolge in die Pager-Adresse eines Benutzers einfügt, um beliebigen Code auszuführen."
        }
      ],
      "release_date": "2022-09-01T22:00:00.000+00:00",
      "title": "CVE-2022-46303"
    }
  ]
