  "vulnerabilities": [
    {
      "cve": "CVE-2021-33159",
      "notes": [
        {
          "category": "description",
          "text": "In der Intel Chipset Firmware existieren mehrere Schwachstellen. Diese sind auf Fehler bei der Implementierung eines Authentisierungsalgorithmus, Logik-Fehler, unzureichendes Überprüfen von Parametern, sowie weitere nicht-genannte Quellen zurückzuführen. Ein entfernter, anonymer Angreifer kann diese Schwachstellen ausnutzen, um seine Privilegien zu erweitern, einen Denial of Service Zustand herbeizuführen oder Dateien zu manipulieren."
        }
      ],
      "product_status": {
        "known_affected": [
          "T006520",
          "T023191",
          "T014381",
          "T026506"
        ]
      },
      "release_date": "2022-11-08T23:00:00.000+00:00",
