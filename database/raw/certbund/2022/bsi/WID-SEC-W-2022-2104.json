  "vulnerabilities": [
    {
      "cve": "CVE-2022-41800",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in F5 BIG-IP. Im Appliance-Modus kann ein authentifizierter Benutzer, dem die Rolle des Administrators zugewiesen wurde, die Einschränkungen des Appliance-Modus umgehen, indem er einen nicht offengelegten iControl REST-Endpunkt verwendet. Ein privilegierter Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitseinstellungen zu umgehen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T025350",
