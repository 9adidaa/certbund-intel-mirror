  "vulnerabilities": [
    {
      "cve": "CVE-2024-6915",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in JFrog Artifactory aufgrund einer unsachgemäßen Eingabevalidierung, die es ermöglicht, einen Cache-Poisoning-Angriff durchzuführen. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um den Cache von Artefakten zu vergiften und so die Integrität von Software, die aus diesen Repositories bereitgestellt wird, zu gefährden."
        }
      ],
      "release_date": "2024-08-05T22:00:00.000+00:00",
