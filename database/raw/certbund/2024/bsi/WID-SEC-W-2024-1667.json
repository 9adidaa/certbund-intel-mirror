  "vulnerabilities": [
    {
      "cve": "CVE-2024-20419",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Cisco Smart Software Manager On-Prem aufgrund der unsachgemäßen Implementierung des Passwort-Änderungsprozesses, der es erlaubt, das Passwort eines beliebigen Benutzers, einschließlich administrativer Benutzer, zu ändern. Durch das Senden von manipulierten HTTP-Anfragen kann ein entfernter, anonymer Angreifer diese Schwachstelle ausnutzen, um seine Privilegien zu erweitern und die Kontrolle über das System zu übernehmen."
        }
      ],
      "release_date": "2024-07-17T22:00:00.000+00:00",
      "title": "CVE-2024-20419"
    }
  ]
