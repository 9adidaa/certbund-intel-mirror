  "vulnerabilities": [
    {
      "cve": "CVE-2024-37899",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in XWiki aufgrund der unsachgemäßen Behandlung von Benutzerprofilen, die es ermöglicht, dass der Inhalt des Benutzerprofils mit den Rechten des Administrators ausgeführt wird, wenn das Konto von einem Administrator deaktiviert wurde. Ein entfernter, authentisierter Angreifer kann diese Sicherheitslücke ausnutzen, um bösartigen Code mit erhöhten Rechten auszuführen. Eine erfolgreiche Ausnutzung erfordert eine spezielle Benutzerinteraktion."
        }
      ],
      "release_date": "2024-06-20T22:00:00.000+00:00",
      "title": "CVE-2024-37899"
    }
