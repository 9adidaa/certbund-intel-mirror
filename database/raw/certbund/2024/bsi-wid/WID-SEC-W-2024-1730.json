  "vulnerabilities": [
    {
      "cve": "CVE-2024-5461",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Broadcom Brocade Switch und Broadcom Fabric OS. Diese Fehler besteht aufgrund einer unsachgemäßen Eingabevalidierung in der SNMP-Implementierung, was zu einer Befehls- oder Parameterinjektion führt. Ein Angreifer aus einem angrenzenden Netzwerk kann diese Schwachstelle ausnutzen, um beliebigen Code mit Root-Rechten auszuführen."
        }
      ],
      "product_status": {
        "known_affected": [
          "978054",
          "T005119"
        ]
      },
      "release_date": "2024-07-30T22:00:00.000+00:00",
      "title": "CVE-2024-5461"
    },
    {
      "cve": "CVE-2024-5462",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Broadcom Fabric OS und Broadcom Brocade Switch. Dieser Fehler besteht aufgrund der fehlenden Verschlüsselung von SNMP-Passwörtern in FOS-Konfigurationseinstellungen, was dazu führt, dass sensible SNMP-Passwörter im Klartext in Konfigurationsdateien oder Support Captures offengelegt werden. Ein Angreifer aus einem angrenzenden Netzwerk kann diese Schwachstelle ausnutzen, um Passwörter offenzulegen und die offengelegten Anmeldeinformationen zu verwenden, um auf begrenzte SNMP-MIB-Objekte zuzugreifen und diese möglicherweise zu ändern."
        }
      ],
      "product_status": {
        "known_affected": [
          "978054",
          "T005119"
        ]
      },
      "release_date": "2024-07-30T22:00:00.000+00:00",
      "title": "CVE-2024-5462"
    }
  ]
