  "vulnerabilities": [
    {
      "cve": "CVE-2024-1135",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in IBM Business Automation Workflow Machine Learning Server. Dieser Fehler betrifft die Gunicorn-Komponente aufgrund der unsachgemäßen Analyse der HTTP-Transfer-Encoding-Header, was zum Schmuggeln von HTTP-Anfragen führt. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um die Interpretation von HTTP-Anfragen zu manipulieren, was zu Web-Cache-Poisoning, zur Umgehung des Webanwendungsschutzes und zu XSS-Angriffen führen kann."
        }
      ],
      "release_date": "2024-06-20T22:00:00.000+00:00",
      "title": "CVE-2024-1135"
    }
  ]
