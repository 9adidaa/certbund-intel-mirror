  "vulnerabilities": [
    {
      "cve": "CVE-2024-33665",
      "notes": [
        {
          "category": "description",
          "text": "In Camunda existiert eine Cross-Site Scripting Schwachstelle. HTML und Script-Eingaben werden im angular-translate-Plugin nicht ordnungsgemäß überprüft, bevor sie an den Benutzer zurückgegeben werden. Ein entfernter, anonymer Angreifer kann durch Ausnutzung dieser Schwachstelle beliebigen HTML- und Script-Code durch den Browser des Benutzers im Kontext der betroffenen Seite ausführen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "release_date": "2024-07-01T22:00:00.000+00:00",
      "title": "CVE-2024-33665"
    }
  ]
