  "vulnerabilities": [
    {
      "cve": "CVE-2024-37281",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Kibana aufgrund einer unsachgemäßen Behandlung von Anfragen. Dies ermöglicht es einem Benutzer mit der Viewer-Rolle, die Instanz zum Absturz zu bringen, indem er eine große Anzahl von böswillig gestalteten Anfragen an einen bestimmten Endpunkt sendet. Ein entfernter, authentisierter Angreifer kann diese Schwachstelle ausnutzen, um einen Denial-of-Service-Zustand zu erzeugen."
        }
      ],
      "release_date": "2024-07-30T22:00:00.000+00:00",
      "title": "CVE-2024-37281"
    }
  ]
