  "vulnerabilities": [
    {
      "cve": "CVE-2024-24940",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in JetBrains IntelliJ IDEA. Dieser Fehler besteht aufgrund eines Path-Traversal Problems beim Entpacken von Archiven. Ein lokaler Angreifer kann diese Schwachstelle ausnutzen, um Dateien zu manipulieren. Eine erfolgreiche Ausnutzung erfordert eine Benutzerinteraktion."
        }
      ],
      "release_date": "2024-02-06T23:00:00.000+00:00",
      "title": "CVE-2024-24940"
    },
    {
      "cve": "CVE-2024-24941",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in JetBrains IntelliJ IDEA. Dieser Fehler besteht aufgrund eines Authentifizierungs-Tokens, das an eine ungeeignete URL weitergeleitet wird. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um vertrauliche Informationen offenzulegen. Eine erfolgreiche Ausnutzung erfordert eine Benutzerinteraktion."
        }
      ],
      "release_date": "2024-02-06T23:00:00.000+00:00",
      "title": "CVE-2024-24941"
    }
  ]
