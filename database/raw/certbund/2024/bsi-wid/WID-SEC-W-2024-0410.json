  "vulnerabilities": [
    {
      "cve": "CVE-2023-45860",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Hazelcast. Dieser Fehler besteht in der SQL-Mapping-Funktion des CSV File Source Connectors aufgrund einer unsachgemäßen Berechtigungsprüfung, die es erlaubt, auf Daten aus Dateien zuzugreifen, die auf dem Dateisystem eines Mitglieds gespeichert sind. Ein entfernter, authentifizierter Angreifer kann diese Schwachstelle ausnutzen, um vertrauliche Informationen offenzulegen."
        }
      ],
      "release_date": "2024-02-18T23:00:00.000+00:00",
      "title": "CVE-2023-45860"
    }
  ]
