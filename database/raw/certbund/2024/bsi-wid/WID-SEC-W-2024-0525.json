  "vulnerabilities": [
    {
      "cve": "CVE-2024-1908",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Microsoft GitHub Enterprise. Dieser Fehler besteht in GitHub Connect aufgrund einer unsachgemäßen Rechteverwaltung, die es ermöglicht, ein Download-Token zu verwenden, um private Repository-Daten abzurufen. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsvorkehrungen zu umgehen."
        }
      ],
      "release_date": "2024-02-29T23:00:00.000+00:00",
      "title": "CVE-2024-1908"
    }
