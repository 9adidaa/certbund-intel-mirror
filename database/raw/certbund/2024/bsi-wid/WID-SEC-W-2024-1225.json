  "vulnerabilities": [
    {
      "cve": "CVE-2024-20293",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Cisco Firepower und Cisco ASA (Adaptive Security Appliance). Dieser Fehler besteht bei der Aktivierung einer Zugriffskontrollliste (ACL) aufgrund eines Logikfehlers, der auftritt, wenn eine ACL in der laufenden Konfiguration von inaktiv zu aktiv wechselt. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um konfigurierte ACL-Schutzmaßnahmen zu umgehen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T035033",
          "T035032"
        ]
      },
      "release_date": "2024-05-22T22:00:00.000+00:00",
      "title": "CVE-2024-20293"
    },
    {
      "cve": "CVE-2024-20355",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Cisco Firepower und Cisco ASA (Adaptive Security Appliance). Dieser Fehler besteht in der Implementierung von SAML 2.0 Single Sign-On (SSO) für Remote Access VPN-Dienste aufgrund einer unsachgemäßen Trennung von Autorisierungsdomänen. Ein authentifizierter Angreifer mit gültigen Anmeldedaten für den Fernzugriffs-VPN kann diese Schwachstelle ausnutzen, um Sicherheitsmaßnahmen zu umgehen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T035033",
          "T035032"
        ]
      },
      "release_date": "2024-05-22T22:00:00.000+00:00",
      "title": "CVE-2024-20355"
    }
  ]
