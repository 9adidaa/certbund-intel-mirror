  "vulnerabilities": [
    {
      "cve": "CVE-2024-34693",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Apache Superset. Aufgrund einer ungenügenden Eingabevalidierung kann unter bestimmten Umständen eine MariaDB-Verbindung mit aktivierter local_infile erstellt werden. Ein entfernter, authentisierter Angreifer kann diese Schwachstelle ausnutzen, um einen bestimmten MySQL/MariaDB-SQL-Befehl auszuführen, der Dateien vom Server lesen und deren Inhalt in eine MariaDB-Datenbanktabelle einfügen kann."
        }
      ],
      "release_date": "2024-06-19T22:00:00.000+00:00",
      "title": "CVE-2024-34693"
    }
  ]
