  "vulnerabilities": [
    {
      "cve": "CVE-2024-34085",
      "notes": [
        {
          "category": "description",
          "text": "Es bestehen mehrere Schwachstellen in Siemens JT2Go. Diese Fehler bestehen aufgrund eines Stapelüberlaufs und eines Out-of-Bound-Write-Problems. Durch das Parsen speziell gestalteter XML- oder CGM-Dateien kann ein entfernter, anonymer Angreifer diese Schwachstellen ausnutzen, um beliebigen Code auszuführen. Eine erfolgreiche Ausnutzung erfordert eine Benutzerinteraktion."
        }
      ],
      "release_date": "2024-05-13T22:00:00.000+00:00",
      "title": "CVE-2024-34085"
    },
