  "vulnerabilities": [
    {
      "cve": "CVE-2024-0199",
      "notes": [
        {
          "category": "description",
          "text": "Es bestehen mehrere Schwachstellen in GitLab. Dieser Fehler besteht in der CODEOWNERS-Komponente und ermöglicht böswillige Aktionen, indem eine manipulierte Payload in einem alten Branch verwendet wird. Ein entfernter, authentifizierter Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsmaßnahmen zu umgehen. Eine erfolgreiche Ausnutzung erfordert eine Benutzerinteraktion."
        }
      ],
      "release_date": "2024-03-06T23:00:00.000+00:00",
      "title": "CVE-2024-0199"
    },
    {
      "cve": "CVE-2024-1299",
      "notes": [
        {
          "category": "description",
          "text": "Es bestehen mehrere Schwachstellen in GitLab. Dieser Fehler besteht aufgrund einer fehlerhaften Autorisierung, die es erlaubt, Gruppenzugriffstoken mit Besitzerrechten zu rotieren. Ein entfernter, privilegierter Angreifer kann diese Schwachstelle ausnutzen, um seine Privilegien zu erweitern."
        }
      ],
      "release_date": "2024-03-06T23:00:00.000+00:00",
      "title": "CVE-2024-1299"
    }
  ]
