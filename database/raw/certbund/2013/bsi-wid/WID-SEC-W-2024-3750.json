  "vulnerabilities": [
    {
      "cve": "CVE-2013-1899",
      "notes": [
        {
          "category": "description",
          "text": "In PostgreSQL besteht eine Schwachstelle, welche zur Manipulation von Daten ausgenutzt werden kann. Diese Schwachstelle kann über eine Verbindungsanfrage ausgenutzt werden, welche einen Datenbanknamen enthält, der mit einem \"-\" beginnt. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um Daten innerhalb des Datenverzeichnisses des Server zu beschädigen oder zu löschen."
        }
      ],
      "product_status": {
        "known_affected": [
          "131442",
          "155532",
          "23407",
          "170497",
          "131446",
          "T000897",
          "T000896",
