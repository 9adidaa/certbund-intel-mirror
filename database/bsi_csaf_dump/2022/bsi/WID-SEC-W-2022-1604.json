  "vulnerabilities": [
    {
      "cve": "CVE-2022-41765",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in MediaWiki. Der Fehler besteht, weil reassignEdits die Ergebnisse einer IP-Bereichsprüfung auf Special:Contributions nicht aktualisiert. Ein Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsmaßnahmen zu umgehen."
        }
      ],
      "product_status": {
