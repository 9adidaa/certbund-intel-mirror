  "vulnerabilities": [
    {
      "cve": "CVE-2022-3479",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Mozilla NSS. Der Fehler besteht aufgrund eines Authentifizierungsabsturzes, wenn sich kein Benutzerzertifikat in der Datenbank befindet. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial-of-Service-Zustand zu verursachen."
        }
      ],
