  "vulnerabilities": [
    {
      "cve": "CVE-2022-31123",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Grafana. Es besteht ein Fehler in der Versionskontrolle, in Folge ist es möglich die Signaturkontrolle zu umgehen. Ein lokaler Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsvorkehrungen zu umgehen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "product_status": {
        "known_affected": [
