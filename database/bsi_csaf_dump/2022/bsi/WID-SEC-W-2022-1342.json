  "vulnerabilities": [
    {
      "cve": "CVE-2022-34165",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in IBM WebSphere Application Server. Nutzereingaben werden nur ungenügend validiert, was eine HTTP Header Injection ermöglicht. Ein authentisierter Angreifer kann diese Schwachstelle ausnutzen, um unterschiedliche Angriffe, wie z.B. Cache Poisoning oder Cross Site Scripting, auszuführen."
        }
      ],
