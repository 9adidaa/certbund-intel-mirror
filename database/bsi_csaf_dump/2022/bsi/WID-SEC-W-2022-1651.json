  "vulnerabilities": [
    {
      "cve": "CVE-2022-42703",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle im Linux Kernel. Diese ist auf einen Use-after-Free-Fehler zurückzuführen. Ein Angreifer kann diese Schwachstelle ausnutzen, um nicht näher spezifizierte Auswirkungen zu verursachen."
        }
      ],
