  "vulnerabilities": [
    {
      "cve": "CVE-2023-22518",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Atlassian Confluence. Diese hängt mit einer fehlerhaften Authentisierung zusammen. Es ist möglich Confluence zurückzusetzen und ein Confluence-Instanzadministratorkonto zu erstellen. Ein entfernter, anonymer Angreifer kann dann alle administrativen Aktionen durchführen, die dem Confluence-Instanzadministrator zur Verfügung stehen, was zu einem vollständigen Verlust der Vertraulichkeit, Integrität und Verfügbarkeit führt."
        }
      ],
      "release_date": "2023-10-30T23:00:00.000+00:00",
      "title": "CVE-2023-22518"
    }
  ]
