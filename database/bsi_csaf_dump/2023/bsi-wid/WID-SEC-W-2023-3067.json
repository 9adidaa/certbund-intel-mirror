  "vulnerabilities": [
    {
      "cve": "CVE-2023-45142",
      "notes": [
        {
          "category": "description",
          "text": "In Red Hat OpenShift besteht eine Schwachstelle aufgrund eines Speicherlecks im \"otelhttp handler\" von \"open-telemetry\". Ein Angreifer kann dies für einen Denial-of-Service-Angriff ausnutzen, indem er viele speziell gestaltete Anfragen sendet."
        }
      ],
      "product_status": {
        "known_affected": [
