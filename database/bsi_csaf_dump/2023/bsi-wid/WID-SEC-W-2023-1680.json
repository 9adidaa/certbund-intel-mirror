  "vulnerabilities": [
    {
      "cve": "CVE-2023-37453",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle im Linux-Kernel im USB-Subsystem. Die Ursache ist ein out-of-bounds in read_descriptors in drivers/usb/core/sysfs.c. Ein Angreifer kann einen Denial-of-Service-Zustand erzeugen."
        }
      ],
