  "vulnerabilities": [
    {
      "cve": "CVE-2023-48308",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Nextcloud. Es ist möglich auf eine aktive Sitzung eines anderen Benutzers zuzugreifen und durch das Senden von Aufrufen direkt an die API Arbeitsabläufe zu löschen und zu ändern. Ein Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsvorkehrungen zu umgehen."
        }
      ],
      "release_date": "2023-12-17T23:00:00.000+00:00",
      "title": "CVE-2023-48308"
    },
    {
      "cve": "CVE-2023-49790",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Nextcloud. Wenn ein (Reverse-)Proxy als vertrauenswürdiger Proxy konfiguriert ist, kann der Server dazu verleitet werden, eine falsche Remote-Adresse für einen Angreifer zu lesen, so dass dieser mehr Authentifizierungsversuche als beabsichtigt durchführen kann. Ein Angreifer kann diese Schwachstelle ausnutzen, um den Bruteforce-Schutz zu umgehen."
        }
      ],
      "release_date": "2023-12-17T23:00:00.000+00:00",
      "title": "CVE-2023-49790"
    },
    {
      "cve": "CVE-2023-49791",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in der Nextcloud Calendar App. Es besteht ein Problem bei der Verarbeitung von Ausnahmebedingungen. Ein Angreifer kann diese Schwachstelle ausnutzen, wenn er beim Bearbeiten eines Kalendertermins eine Ausnahme erzeugt und sich so Zugang zu Stacktrace und internen Pfaden des Servers verschaffen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "release_date": "2023-12-17T23:00:00.000+00:00",
      "title": "CVE-2023-49791"
    },
    {
      "cve": "CVE-2023-49792",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Nextcloud im Files iOS Paket. Die Anwendung kann ohne Angabe des 4-stelligen PIN-Codes genutzt werden. Ein Angreifer mit physischem Zugriff kann diese Schwachstelle ausnutzen, um Sicherheitsvorkehrungen zu umgehen."
        }
      ],
      "release_date": "2023-12-17T23:00:00.000+00:00",
      "title": "CVE-2023-49792"
    }
  ]
