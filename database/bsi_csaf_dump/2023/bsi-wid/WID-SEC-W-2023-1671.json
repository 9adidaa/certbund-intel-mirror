  "vulnerabilities": [
    {
      "cve": "CVE-2023-36934",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Progress Software MOVEit. Dabei handelt es sich um eine SQL-Injection-Schwachstelle in der MOVEit Transfer-Webanwendung. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um MOVEit-Datenbankinhalte zu ändern oder offenzulegen."
        }
      ],
      "release_date": "2023-07-05T22:00:00.000+00:00",
      "title": "CVE-2023-36934"
    },
    {
      "cve": "CVE-2023-36933",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Progress Software MOVEit. Es ist möglich eine Methode aufzurufen, die zu einer unbehandelten Ausnahme führt. Das Auslösen dieses Workflows kann dazu führen, dass die MOVEit Transfer-Anwendung unerwartet beendet wird. Ein Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service zu verursachen."
        }
      ],
      "release_date": "2023-07-05T22:00:00.000+00:00",
      "title": "CVE-2023-36933"
    },
