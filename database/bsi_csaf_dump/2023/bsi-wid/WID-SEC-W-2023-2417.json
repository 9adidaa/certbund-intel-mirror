  "vulnerabilities": [
    {
      "cve": "CVE-2023-42660",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in der Progress Software MOVEit. Dieser Fehler besteht in der Transfer Machine Interface aufgrund eines SQL-Injection-Problems. Ein authentifizierter Angreifer kann diese Schwachstelle ausnutzen, um Dateien zu manipulieren oder um vertrauliche Informationen offenzulegen."
        }
      ],
      "release_date": "2023-09-20T22:00:00.000+00:00",
      "title": "CVE-2023-42660"
    },
    {
      "cve": "CVE-2023-42656",
      "notes": [
        {
          "category": "description",
          "text": "In Progress Software MOVEit existiert eine Cross-Site Scripting Schwachstelle. HTML und Script-Eingaben werden in Transfer nicht ordnungsgemäß überprüft, bevor sie an den Benutzer zurückgegeben werden. Ein entfernter, anonymer Angreifer kann durch Ausnutzung dieser Schwachstelle beliebigen HTML- und Script-Code durch den Browser des Benutzers im Kontext der betroffenen Seite ausführen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "release_date": "2023-09-20T22:00:00.000+00:00",
      "title": "CVE-2023-42656"
    },
    {
      "cve": "CVE-2023-40043",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in der Progress Software MOVEit. Dieser Fehler besteht im Übertragungssystem aufgrund eines SQL-Injection-Problems. Ein privilegierter Angreifer kann diese Schwachstelle ausnutzen, um Dateien zu manipulieren oder um vertrauliche Informationen offenzulegen."
        }
      ],
      "release_date": "2023-09-20T22:00:00.000+00:00",
      "title": "CVE-2023-40043"
    }
  ]
