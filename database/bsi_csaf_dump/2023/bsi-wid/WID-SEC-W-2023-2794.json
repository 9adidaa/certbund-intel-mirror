  "vulnerabilities": [
    {
      "cve": "CVE-2023-20886",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in VMware Workspace One UEM. Der Fehler besteht aufgrund eines offenen Redirect-Fehlers. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um die SAML-Antwort eines Benutzers abzurufen und sich als der Benutzer des Opfers anzumelden. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "release_date": "2023-10-31T23:00:00.000+00:00",
      "title": "CVE-2023-20886"
    }
