  "vulnerabilities": [
    {
      "cve": "CVE-2023-2478",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in GitLab. Unter bestimmten Bedingungen kann jedes GitLab-Benutzerkonto auf der Instanz einen GraphQL-Endpunkt verwenden, um einen bösartigen Runner an ein beliebiges Projekt auf der Instanz anzuhängen. Ein authentisierter Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsvorkehrungen zu umgehen."
        }
      ],
      "release_date": "2023-05-07T22:00:00.000+00:00",
      "title": "CVE-2023-2478"
    }
