  "vulnerabilities": [
    {
      "cve": "CVE-2022-31081",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in IBM AIX und IBM VIOS. Diese besteht in libwww-perl und ermöglicht Web-Cache Poisoning. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um den Schutz der Web-Anwendungs-Firewall zu umgehen und XSS-Angriffe durchzuführen."
        }
      ],
