  "vulnerabilities": [
    {
      "cve": "CVE-2023-43121",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Extreme Networks ExtremeXOS. Dieser Fehler besteht in der Chalet-Anwendung, die das Durchqueren von Verzeichnissen ermöglicht, was dazu führt, dass jede Datei auf dem System gelesen werden kann. Ein lokaler Angreifer kann diese Schwachstelle ausnutzen, um vertrauliche Informationen offenzulegen."
        }
      ],
      "release_date": "2023-10-04T22:00:00.000+00:00",
      "title": "CVE-2023-43121"
    },
    {
      "cve": "CVE-2023-43120",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Extreme Networks ExtremeXOS. Durch die Verwendung des Telnet-Tools kann eine HTTP-Anfrage gefälscht werden. Ein entfernter authentifizierter Angreifer kann diese Schwachstelle ausnutzen, um seine Privilegien zu erweitern."
        }
      ],
      "release_date": "2023-10-04T22:00:00.000+00:00",
      "title": "CVE-2023-43120"
    },
    {
      "cve": "CVE-2023-43119",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Extreme Networks ExtremeXOS. Dieser Fehler existiert im Redis-Server, der es erlaubt, telnet zu benutzen, um beliebige Dateisystemoperationen mit Root-Rechten durchzuführen. Ein entfernter Angreifer kann diese Schwachstelle ausnutzen, um seine Privilegien zu erweitern."
        }
      ],
      "release_date": "2023-10-04T22:00:00.000+00:00",
      "title": "CVE-2023-43119"
    },
    {
      "cve": "CVE-2023-43118",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Extreme Networks ExtremeXOS. Dieser Fehler besteht in den Endpunkten der Chalet-Anwendung, die einen authentifizierten Benutzer dazu bringen kann, Aktionen über eine Cross-Site-Request-Forgery auszuführen. Ein Angreifer kann diese Schwachstelle zur Ausführung von beliebigem Code ausnutzen. Eine erfolgreiche Ausnutzung erfordert eine Benutzerinteraktion."
        }
      ],
      "release_date": "2023-10-04T22:00:00.000+00:00",
      "title": "CVE-2023-43118"
    }
  ]
