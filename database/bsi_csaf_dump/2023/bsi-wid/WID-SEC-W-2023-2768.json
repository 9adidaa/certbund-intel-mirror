  "vulnerabilities": [
    {
      "cve": "CVE-2023-46853",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in memcached. Dieser Fehler tritt bei der Verarbeitung von Proxy-Anfragen im Proxy-Modus auf, wenn \\n anstelle von \\r\\n verwendet wird, was zu einem Off-by-One-Fehler führt. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um unbekannte Auswirkungen zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T000126"
        ]
      },
      "release_date": "2023-10-29T23:00:00.000+00:00",
      "title": "CVE-2023-46853"
    },
    {
      "cve": "CVE-2023-46852",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in memcached. Dieser Fehler besteht bei der Verarbeitung von Multiget-Anfragen im Proxy-Modus. Durch die Verwendung von vielen Leerzeichen nach dem \"get\"-Teilstring kann ein entfernter, anonymer Angreifer diese Schwachstelle ausnutzen, um unbekannte Auswirkungen zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T000126"
        ]
      },
