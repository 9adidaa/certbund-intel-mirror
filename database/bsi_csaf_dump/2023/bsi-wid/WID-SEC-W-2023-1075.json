  "vulnerabilities": [
    {
      "cve": "CVE-2023-24512",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Arista EOS. Der Fehler besteht aufgrund einer unsachgemäßen Zugriffskontrolle. Ein entfernter, authentisierter Angreifer mit der Berechtigung, gNMI-Anfragen durchzuführen, kann diese Schwachstelle ausnutzen, um beliebige Konfigurationen im Switch zu aktualisieren. Die Situation tritt nur auf, wenn der Streaming Telemetry Agent aktiviert ist und der gNMI-Zugriff auf dem Agent konfiguriert ist."
        }
      ],
      "release_date": "2023-04-25T22:00:00.000+00:00",
      "title": "CVE-2023-24512"
    }
  ]
