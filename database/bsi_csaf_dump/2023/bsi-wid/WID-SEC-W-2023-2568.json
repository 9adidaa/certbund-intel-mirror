  "vulnerabilities": [
    {
      "cve": "CVE-2023-20235",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Cisco IOS XE. Die Schwachstelle ist darauf zurückzuführen, dass Docker Container im \"application development\" Modus privilegiert laufen können und eine Interaktion per Kommandozeile mit ihnen möglich ist. Ein entfernter, authentisierter Angreifer kann diese Schwachstelle ausnutzen, um Administratorrechte zu erlangen."
        }
      ],
      "release_date": "2023-10-04T22:00:00.000+00:00",
      "title": "CVE-2023-20235"
    }
