  "vulnerabilities": [
    {
      "cve": "CVE-2023-2177",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle im Linux Kernel. Im SCTP Netzwerk Protokoll besteht in net/sctp/stream_sched.c ein NULL Pointer Dereferenz Problem. Ein lokaler Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service zu verursachen."
        }
      ],
