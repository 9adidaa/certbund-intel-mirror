  "vulnerabilities": [
    {
      "cve": "CVE-2023-20884",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in VMware Workspace One Access, VMware Identity Manager und VMware Cloud Foundation. Aufgrund einer unsachgemäßen Pfadbehandlung besteht die Möglichkeit einer unsicheren Redirection. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um das Opfer unbemerkt auf eine beliebige Seite umzuleiten, und so Informationen offenlegen."
        }
      ],
      "release_date": "2023-05-30T22:00:00.000+00:00",
      "title": "CVE-2023-20884"
    }
  ]
