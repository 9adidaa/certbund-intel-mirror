  "vulnerabilities": [
    {
      "cve": "CVE-2023-41041",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Graylog. In einem Graylog-Cluster mit mehreren Knoten kann eine Benutzersitzung, nachdem sich ein Benutzer ausdrücklich abgemeldet hat, noch für API-Anfragen verwendet werden, bis sie ihre ursprüngliche Ablaufzeit erreicht hat. Ein Angreifer kann diese Schwachstelle ausnutzen, um Informationen offenzulegen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "release_date": "2023-08-30T22:00:00.000+00:00",
      "title": "CVE-2023-41041"
    }
  ]
