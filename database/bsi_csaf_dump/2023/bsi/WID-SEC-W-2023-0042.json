  "vulnerabilities": [
    {
      "cve": "CVE-2022-42435",
      "notes": [
        {
          "category": "description",
          "text": "Eine Cross-Site Request-Forgery-Schwachstelle besteht in IBM Business Automation Workflow. Dieses Problem wird z.B. durch unsachgemäße Formularverarbeitung verursacht. Dies ermöglicht es einem Angreifer, den Webbrowser des Opfers dazu zu bringen, unbeabsichtigte Anfragen an IBM Business Automation Workflow zu stellen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
