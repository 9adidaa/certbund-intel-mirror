  "vulnerabilities": [
    {
      "cve": "CVE-2023-2181",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in GitLab. Diese ist auf einen Fehler in \"refs/replace\" zurückzuführen, welche es ermöglicht Code in einen Merge Request zu schmuggeln, welche dann nicht in der UI angezeigt wird. Ein entfernter, authentisierter Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsvorkehrungen zu umgehen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "release_date": "2023-05-10T22:00:00.000+00:00",
      "title": "CVE-2023-2181"
    }
  ]
