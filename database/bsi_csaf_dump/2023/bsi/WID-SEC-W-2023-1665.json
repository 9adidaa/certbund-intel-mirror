  "vulnerabilities": [
    {
      "cve": "CVE-2023-34261",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Kyocera TASKalfa 4053ci Printern. Die Login-Funktion gibt unterschiedliche Antworten zurück, abhängig davon, ob der Benutzername gültig ist oder nicht. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um Informationen offenzulegen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T029669",
          "T029668"
        ]
      },
      "release_date": "2023-07-04T22:00:00.000+00:00",
      "title": "CVE-2023-34261"
    },
    {
      "cve": "CVE-2023-34260",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Kyocera TASKalfa 4053ci Printern. Diese steht im Zusammenhang mit der zuvor beschriebenen Path-Traversal-Schwachstelle. Wird statt einer Datei ein Verzeichnis angefordert, reagiert der Webdienst nicht mehr und muss neu gestartet werden. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T029669",
          "T029668"
        ]
      },
      "release_date": "2023-07-04T22:00:00.000+00:00",
      "title": "CVE-2023-34260"
    },
    {
      "cve": "CVE-2023-34259",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Kyocera TASKalfa 4053ci Printern. Hierbei handelt es sich um eine Path Traversal Schwachstelle. Ein entfernter, anonymer Angreifer kann diese Ausnutzen um auf beliebige Dateien zuzugreifen. Aufgrund der Tatsache, dass der Webservice als Benutzer root läuft, ist es möglich, auf alle Dateien (z.B. /etc/shadow) auf dem Gerät zuzugreifen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T029669",
          "T029668"
        ]
      },
      "release_date": "2023-07-04T22:00:00.000+00:00",
      "title": "CVE-2023-34259"
    }
  ]
