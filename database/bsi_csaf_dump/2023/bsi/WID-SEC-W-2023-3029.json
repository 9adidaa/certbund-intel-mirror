  "vulnerabilities": [
    {
      "cve": "CVE-2023-6218",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in der Progress Software MOVEit. Dies ist auf die Möglichkeit der Privilegienerweiterung im Zusammenhang mit Gruppenadministratoren zurückzuführen. Ein entfernter, authentifizierter Angreifer kann diese Schwachstelle ausnutzen, um seine Rechte zu erweitern."
        }
      ],
      "release_date": "2023-11-29T23:00:00.000+00:00",
      "title": "CVE-2023-6218"
    },
    {
      "cve": "CVE-2023-6217",
      "notes": [
        {
          "category": "description",
          "text": "In Progress Software MOVEit existiert eine Cross-Site Scripting Schwachstelle. HTML und Script-Eingaben werden nicht ordnungsgemäß überprüft, bevor sie an den Benutzer zurückgegeben werden. Ein entfernter, anonymer Angreifer kann durch Ausnutzung dieser Schwachstelle beliebigen HTML- und Script-Code durch den Browser des Benutzers im Kontext der betroffenen Seite ausführen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "release_date": "2023-11-29T23:00:00.000+00:00",
      "title": "CVE-2023-6217"
    }
  ]
