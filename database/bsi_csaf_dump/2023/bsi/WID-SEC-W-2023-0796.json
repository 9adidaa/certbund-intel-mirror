  "vulnerabilities": [
    {
      "cve": "CVE-2023-0225",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Samba. Der Fehler besteht aufgrund einer unvollständigen Zugriffsprüfung auf das Attribut dnsHostName. Ein entfernter, authentisierter Angreifer kann diese Schwachstelle ausnutzen, um dieses Attribut von jedem Objekt im Verzeichnis zu löschen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T010641",
          "T038611",
          "T012167",
          "74185",
          "T000126",
          "T026996",
          "T026994",
          "T023598",
          "T026995",
