  "vulnerabilities": [
    {
      "cve": "CVE-2021-39144",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Veritas NetBackup. Der Fehler besteht aufgrund eines nicht authentifizierten Endpunkts, der XStream zur Serialisierung von Eingaben in NetBackup verwendet. Ein entfernter, anonymer Angreifer kann diese Schwachstelle zur Ausführung von beliebigem Code mit Root-Rechten ausnutzen. Die betroffene Komponente ist laut Veritas in den angegebenen Produkten enthalten, aber im Zusammenhang mit den Veritas Produkten nicht ausnutzbar."
        }
      ],
      "release_date": "2023-03-29T22:00:00.000+00:00",
      "title": "CVE-2021-39144"
    }
  ]
