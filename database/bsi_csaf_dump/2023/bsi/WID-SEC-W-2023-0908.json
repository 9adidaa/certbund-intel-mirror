  "vulnerabilities": [
    {
      "cve": "CVE-2023-28489",
      "notes": [
        {
          "category": "description",
          "text": "Eine Schwachstelle für die Einschleusung von Befehlen besteht in mehreren Firmware-Versionen von Siemens SICAM A8000-Geräten. Die Geräte sind über den Webserver-Port 443/tcp verwundbar, wenn der Parameter \"Remote Operation\" aktiviert ist, der standardmäßig deaktiviert ist. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um beliebigen Programmcode auszuführen."
        }
      ],
      "release_date": "2023-04-10T22:00:00.000+00:00",
      "title": "CVE-2023-28489"
    }
