  "vulnerabilities": [
    {
      "cve": "CVE-2023-20169",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Cisco Nexus. Dieser Fehler besteht in Nexus-Switches der 3000er-Serie und Cisco Nexus-Switches der 9000er-Serie im Standalone-NX-OS-Modus aufgrund einer unzureichenden Eingabevalidierung beim Parsen eines IS-IS-Eingangs-Pakets. Ein nicht authentifizierter Angreifer kann diese Schwachstelle ausnutzen, um einen Denial-of-Service-Zustand zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T029478",
          "T029477"
        ]
      },
      "release_date": "2023-08-23T22:00:00.000+00:00",
      "title": "CVE-2023-20169"
    },
    {
      "cve": "CVE-2023-20115",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Cisco Nexus. Dieser Fehler besteht in Nexus-Switches der Serien 3000 und 9000 im Standalone-NX-OS-Modus aufgrund einer nicht ordnungsgemäß konfigurierten SFTP-Server-Implementierung. Ein entfernter authentifizierter Angreifer kann diese Schwachstelle ausnutzen, um vertrauliche Informationen offenzulegen oder Daten zu manipulieren."
        }
      ],
