  "vulnerabilities": [
    {
      "cve": "CVE-2023-32313",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in vm2 bezüglich der \"console.log\". Ein Angreifer kann einen Lese- und Schreibzugriff auf die Methode \"node inspect\" erhalten und anschließend die Optionen für \"console.log\" manipulieren."
        }
      ],
