  "vulnerabilities": [
    {
      "cve": "CVE-2023-1859",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle im Linux-Kernel. Der Fehler besteht aufgrund eines Use-after-free-Fehlers in der Funktion xen_9pfs_front_removet in der Komponente Xen transport for 9pfs. Ein lokaler Angreifer kann diese Schwachstelle ausnutzen, um einen Denial-of-Service-Zustand zu verursachen, der möglicherweise zu einem Kernel-Informationsleck führt."
        }
      ],
