  "vulnerabilities": [
    {
      "cve": "CVE-2019-18179",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in OTRS. Ein Angreifer, der als Agent bei OTRS angemeldet ist, kann Tickets auflisten, die anderen Agenten in einer nicht dem Angreifer zugeordneten Warteschlange gehören. In der Folge kann er Informationen offenlegen."
        }
      ],
