  "vulnerabilities": [
    {
      "cve": "CVE-2019-3728",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in RSA BSAFE. RSA BSAFE Crypto-C Micro Edition und RSA BSAFE Micro Edition Suite sind für eine Buffer Over-Read-Schwachstelle bei der Verarbeitung von DSA-Signaturen anfällig. Ein entfernter anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T014381",
          "T015013"
        ]
      },
      "release_date": "2019-09-30T22:00:00.000+00:00",
      "title": "CVE-2019-3728"
    },
    {
      "cve": "CVE-2019-3729",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in der RSA BSAFE Micro Edition Suite. Diese Schwachstelle besteht aufgrund eines Heap-basierten Pufferüberlaufes beim Parsen von ECDSA-Signaturen. Ein Angreifer aus dem angrenzenden Netzwerk kann diese Schwachstelle ausnutzen, um einen Denial of Service zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T014381",
          "T015013"
        ]
      },
      "release_date": "2019-09-30T22:00:00.000+00:00",
      "title": "CVE-2019-3729"
    },
    {
      "cve": "CVE-2019-3730",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in der RSA BSAFE Micro Edition Suite. Hier handelt es sich um eine Fehlermeldungsschwachstelle, die als \"padding oracle attack vulnerability\" bekannt ist. Ein entfernter Angreifer kann diese Schwachstelle ausnutzen, um Informationen offenzulegen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T014381",
          "T015013"
        ]
      },
      "release_date": "2019-09-30T22:00:00.000+00:00",
      "title": "CVE-2019-3730"
    },
