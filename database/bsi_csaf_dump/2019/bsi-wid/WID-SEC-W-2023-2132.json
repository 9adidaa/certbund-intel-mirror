  "vulnerabilities": [
    {
      "cve": "CVE-2019-15139",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in ImageMagick. Die XWD-Image (X Window System Window Dumping File) Parsing-Komponente ermöglicht es einem entfernten anonymen Angreifer, einen Denial-of-Service aufgrund eines Out-of-Bounds Read in ReadXWDImage in coders/xwd.c zu verursachen. Zur erfolgreichen Ausnutzung der Schwachstelle muss der Angreifer sein Opfer dazu bringen eine beschädigte XWD-Image-Datei zu öffnen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T015518",
