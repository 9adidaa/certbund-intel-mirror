  "vulnerabilities": [
    {
      "cve": "CVE-2019-14861",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Samba. Ein authentisieter Benutzer kann den DCE/RPC DNS-Verwaltungsserver abstürzen lassen, indem er Datensätze erstellt, die dem Zonennamen entsprechen."
        }
      ],
