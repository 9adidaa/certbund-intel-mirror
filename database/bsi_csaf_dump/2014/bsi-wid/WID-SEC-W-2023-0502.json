  "vulnerabilities": [
    {
      "cve": "CVE-2010-5298",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in OpenSSL wenn SSL_MODE_RELEASE_BUFFERS aktiviert ist. Ein Angreifer kann eine \"use-after-free\" Schwäche in der ssl3_read_bytes Funktion in s3_pkt.c nutzen, um Daten in andere Verbindungen zu injizieren oder einen Denial of Service Angriff durchzuführen."
        }
      ],
