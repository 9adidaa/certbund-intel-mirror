  "vulnerabilities": [
    {
      "cve": "CVE-2014-3613",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Information Disclosure Schwachstelle in cURL. Ein Fehler bei der Verarbeitung von bestimmten IP Adressen während des Parsens von empfangenen HTTP-Cookies kann von einem Angreifer ausgenutzt werden um ansonsten unzugängliche Cookies auszulesen."
        }
      ],
