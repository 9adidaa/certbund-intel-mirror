  "vulnerabilities": [
    {
      "cve": "CVE-2020-25862",
      "notes": [
        {
          "category": "description",
          "text": "In Wireshark bestehen mehrere Schwachstellen aufgrund von Fehlern in den folgenden Komponenten: MIME Multipart Dissector, TCP Dissector und BLIP Dissector. Ein entfernter anonymer Angreifer kann dies ausnutzen, um einen Denial-of-Service Zustand herbeizuführen. Für eine erfolgreiche Ausnutzung muss der Angreifer einen Benutzer davon überzeugen, eine bösartig gestaltete Paketverfolgungsdatei zu öffnen, oder er muss bösartige Pakete in das Netzwerk des Opfers injizieren."
        }
      ],
      "product_status": {
        "known_affected": [
          "2951",
          "T002207",
          "T000126"
        ]
      },
      "release_date": "2020-09-23T22:00:00.000+00:00",
      "title": "CVE-2020-25862"
    },
