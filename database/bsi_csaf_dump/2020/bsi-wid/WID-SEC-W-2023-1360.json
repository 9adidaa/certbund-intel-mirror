  "vulnerabilities": [
    {
      "cve": "CVE-2020-14058",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Squid bei der Verarbeitung von TLS-Zertifikaten. Diese Schwachstelle besteht nur dann, wenn Squid mit OpenSSL-Funktionen gebaut wurde und Peer- oder Server-Verbindungen für HTTPS-Verkehr und SSL-Bump-Server-Handshakes öffnet. Ein entfernter anonymer Angreifer kann diese Schwachstelle ausnutzen, um beim Öffnen von TLS-Verbindungen mit einem HTTPS Server einen Denial-of-Service-Angriff durchzuführen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T002207",
          "67646",
          "398363"
        ]
      },
      "release_date": "2020-06-21T22:00:00.000+00:00",
      "title": "CVE-2020-14058"
    },
    {
      "cve": "CVE-2020-14059",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Squid aufgrund einer fehlerhaften Synchronisierung bei der Verarbeitung von Objekten in einem SMP-Cache. Ein authentisierter Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service zu verursachen."
        }
      ],
      "product_status": {
