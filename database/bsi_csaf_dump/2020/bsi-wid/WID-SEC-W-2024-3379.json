  "vulnerabilities": [
    {
      "cve": "CVE-2020-7039",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in QEMU. Die Schwachstelle basiert auf einem Fehler im Speichermanagement durch libslirp 4.1.0. Ein lokaler Angreifer kann diese Schwachstelle ausnutzen. um einen Heap-basieren Pufferüberlauf zu verursachen, wodurch beliebiger Programmcode ausgeführt werden kann oder einen Denial of Service Zustand verursacht werden kann."
        }
      ],
      "product_status": {
        "known_affected": [
