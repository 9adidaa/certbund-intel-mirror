  "vulnerabilities": [
    {
      "cve": "CVE-2020-25613",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Ruby. Ungültige Transfer-Encoding-Header werden in WEBrick unsachgemäß behandelt. Ein entfernter anonymer Angreifer kann diese Schwachstelle ausnutzen, um HTTP Requests zu schmuggeln, und somit Sicherheitsvorkehrungen zu umgehen."
        }
      ],
