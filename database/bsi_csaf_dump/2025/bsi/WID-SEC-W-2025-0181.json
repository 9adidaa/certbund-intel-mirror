  "vulnerabilities": [
    {
      "cve": "CVE-2025-0619",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in M-Files Server aufgrund einer unsicheren Passwortwiederherstellung aus der Konfiguration. Ein entfernter, authentisierter Angreifer mit erhöhten Rechten kann diese Schwachstelle ausnutzen, um Kennwörter für externe Verbindungen wiederherzustellen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T040594"
        ]
      },
      "release_date": "2025-01-22T23:00:00.000+00:00",
      "title": "CVE-2025-0619"
    },
    {
      "cve": "CVE-2025-0635",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in M-Files Server. Unter bestimmten Bedingungen kann M-Files Server übermäßig viel Zeit für die Verarbeitung bestimmter Eingabewerte benötigen. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial-of-Service-Zustand zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T040594"
        ]
      },
      "release_date": "2025-01-22T23:00:00.000+00:00",
      "title": "CVE-2025-0635"
    },
    {
      "cve": "CVE-2025-0648",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in M-Files Server. Die Konfiguration des EOT-Datenbanktreibers kann zu einem unerwarteten Absturz führen. Ein entfernter, authentisierter Angreifer mit Tresorverwalterrechten kann diese Schwachstelle ausnutzen, um einen Denial-of-Service-Zustand durch Ändern der EOT-Konfiguration zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T040594"
        ]
      },
      "release_date": "2025-01-22T23:00:00.000+00:00",
