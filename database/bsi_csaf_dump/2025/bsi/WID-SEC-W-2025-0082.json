  "vulnerabilities": [
    {
      "cve": "CVE-2024-21758",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Fortinet FortiWeb. Im Fortiweb-Sicherungsbefehl besteht ein Problem mit einem stapelbasierten Pufferüberlauf. Ein privilegierter Benutzer kann diese Schwachstelle ausnutzen, um über speziell gestaltete CLI-Befehle beliebigen Code auszuführen. Der Angreifer muss in der Lage sein, den Stack-Schutz von FortiWeb zu umgehen, um die Schwachstelle ausnutzen zu können."
        }
      ],
      "product_status": {
        "known_affected": [
          "1601757",
          "1601758"
        ]
      },
      "release_date": "2025-01-14T23:00:00.000+00:00",
      "title": "CVE-2024-21758"
    },
    {
      "cve": "CVE-2024-55593",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Fortinet FortiWeb. Eine unsachgemäße Neutralisierung spezieller Elemente in SQL Abfragen in FortiWeb API Endpoints kann zu einer SQL Injektion führen. Ein entfernter, authentifizierter Angreifer mit Administratorrechten kann diese Schwachstelle ausnutzen, um über manipulierte SQL-Abfragen an Signaturinformationen zu gelangen."
        }
      ],
      "product_status": {
        "known_affected": [
          "1601757",
