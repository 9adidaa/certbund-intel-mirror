  "vulnerabilities": [
    {
      "cve": "CVE-2021-20218",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Red Hat OpenShift. Der Fehler besteht aufgrund eines Fehlers im `copy`-Befehl des fabric8-Kubernetes-Clients. Ein entfernter anonymer Angreifer kann diese Schwachstelle ausnutzen, um Sicherheitsmaßnahmen zu umgehen."
        }
      ],
      "product_status": {
