  "vulnerabilities": [
    {
      "cve": "CVE-2020-11947",
      "notes": [
        {
          "category": "description",
          "text": "Es gibt eine Schwachstelle in QEMU aufgrund eines Heap-Pufferüberlaufs im QEMU-Blocktreiber für iSCSI-Images. Ein Angreifer aus einer Gast-VM kann dies ausnutzen, um Informationen aus dem QEMU-Prozessspeicher über einen Out-of-Bounds-Lesezugriff offenzulegen."
        }
      ],
