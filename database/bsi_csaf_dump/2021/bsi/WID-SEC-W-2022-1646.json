  "vulnerabilities": [
    {
      "cve": "CVE-2021-23369",
      "notes": [
        {
          "category": "description",
          "text": "Es existieren mehrere Schwachstellen in Red Hat OpenShift. Die Fehler besteht in der Komponente node.js-handlebars aufgrund einer fehlenden Prüfung beim Abrufen von Prototyp-Eigenschaften und einem nicht abgespeicherten Wert in der Funktion JavaScriptCompiler.prototype.depthedLookup. Ein entfernter anonymer Angreifer kann diese Schwachstellen zur Ausführung von beliebigem Code ausnutzen."
        }
      ],
      "product_status": {
        "known_affected": [
          "67646",
          "T019704",
          "T025651"
        ]
      },
      "release_date": "2021-06-28T22:00:00.000+00:00",
      "title": "CVE-2021-23369"
    },
