  "vulnerabilities": [
    {
      "cve": "CVE-2021-28041",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in OpenSSH, welche aufgrund eines double-memory-free-Fehlers besteht. Eine Ausnutzung ist möglich, wenn ein Angreifer Zugriff auf den \"Agent-Socket\" besitzt. Ein entfernter, authentisierter Angreifer kann diese Schwachstelle ausnutzen, um beliebigen Programmcode auszuführen."
        }
      ],
