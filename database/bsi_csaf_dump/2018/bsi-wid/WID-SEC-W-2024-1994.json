  "vulnerabilities": [
    {
      "cve": "CVE-2018-20124",
      "notes": [
        {
          "category": "description",
          "text": "Es existieren mehrere Schwachstellen in QEMU aufgrund von Null Pointer Dereferenzen, einem Speicherleck, einem Out-of-Bounds Zugriff und einer Endlosschleife bezüglich der RDMA-Implementierung. Ein Angreifer innerhalb einer Gast-VM kann dies ausnutzen, um einen Denial-of-Service Zustand zu erzeugen."
        }
      ],
      "product_status": {
