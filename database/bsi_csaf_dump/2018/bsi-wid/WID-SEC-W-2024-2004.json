  "vulnerabilities": [
    {
      "cve": "CVE-2018-11806",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine \"heap buffer overflow\" Schwachstelle in QEMU wenn das \"Slirp networking back-end\" fragmentierte Pakete verarbeitet. Ein privilegierter Angreifer in einem Gastsystem kann dies ausnutzen, um den Qemu Prozess zum Absturz zu bringen oder um beliebigen Code auf dem Host mit den Privilegien des Qemu Prozesses auszuführen."
        }
      ],
