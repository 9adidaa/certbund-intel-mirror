  "vulnerabilities": [
    {
      "cve": "CVE-2018-16872",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in QEMU. Ein Angreifer der auf dem Host-Dateisystem, das mit einem Gast geteilt ist, Schreibrechte hat, kann alle Dateien, auf die der Qemu Prozess Zugriff hat, auslesen, und somit Informationen offenlegen."
        }
      ],
      "product_status": {
        "known_affected": [
          "2951",
          "T002207",
          "T000126",
          "T007150",
          "T004914"
        ]
      },
      "release_date": "2018-12-13T23:00:00.000+00:00",
      "title": "CVE-2018-16872"
    },
    {
      "cve": "CVE-2018-19364",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in QEMU. In [hw/9pfs/cofile.c] und [hw/9pfs/9p.c] besteht ein use-after-free Fehler, der zu einer Race Condition führen kann. Ein im Gastsystem angemeldeter Angreifer lann diese Schwachstelle nutzen, um um den QEMU Prozess zum Absturz zu bringen und somit einen Denial of Service verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "2951",
          "T002207",
          "T000126",
          "T007150",
          "T004914"
        ]
      },
      "release_date": "2018-12-13T23:00:00.000+00:00",
      "title": "CVE-2018-19364"
    },
    {
      "cve": "CVE-2018-20123",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in QEMU. Aufgrund der Art und Weise wie QUEMU das paravirtuelle RDMA-Geräts von VmWare initialisiert, entsteht ein Speicherfehler. Ein am Gastsystem angemeldeter Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service für den Host zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "2951",
          "T002207",
          "T000126",
          "T007150",
          "T004914"
        ]
      },
      "release_date": "2018-12-13T23:00:00.000+00:00",
      "title": "CVE-2018-20123"
    }
  ]
