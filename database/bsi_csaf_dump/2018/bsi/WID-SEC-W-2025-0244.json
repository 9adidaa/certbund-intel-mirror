  "vulnerabilities": [
    {
      "cve": "CVE-2017-14461",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Dovecot. Speziell bearbeitete E-Mail Adressen werden unsachgemäß verarbeitet. Werden solche Mails über SMTP zugestellt und von MTA an Dovecot weitergegeben, kann ein Lesen außerhalb des zulässigen Bereichs möglich werden, was zu einer möglichen Offenlegung vertraulicher Informationen oder zu einem Denial of Service führen kann."
        }
      ],
      "product_status": {
        "known_affected": [
          "T010559",
          "T011082",
          "T002207",
          "T027843",
          "T010560",
          "T011917"
        ]
      },
      "release_date": "2018-03-05T23:00:00.000+00:00",
      "title": "CVE-2017-14461"
    },
    {
      "cve": "CVE-2017-15130",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Dovecot. Ein entfernter anonymer Angreifer der in der Lage ist, zufällige SNI-Servernamen zu generieren, kann die TLS-SNI-Konfigurationssuche dazu ausnutzen, um eine übermäßige Speicherauslastung zu verursachen und damit einem Neustart des Prozesses herbeizuführen."
        }
      ],
      "product_status": {
