  "vulnerabilities": [
    {
      "cve": "CVE-2018-16646",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in poppler. Ein entfernter anonymer Angreifer kann diese Schwachstelle in der Parser::getObj() Funktion in Parser.cc ausnutzen, um einen Denial of Service zu verursachen. Zur erfolgreichen Ausnutzung dieser Schwachstelle muss der Angreifer den Benutzer dazu bringen, eine modifizierte Datei zu öffnen."
        }
      ],
