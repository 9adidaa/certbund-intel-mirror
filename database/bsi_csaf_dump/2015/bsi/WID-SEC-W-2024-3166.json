  "vulnerabilities": [
    {
      "cve": "CVE-2015-3276",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in OpenLDAP. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um Daten zu manipulieren. Die Schwachstelle besteht in der Art und Weise wie OpenLDAP OpenSSL Cipher Strings verarbeitet. In der Folge kann OpenLDAP Ciphers nutzen, die eigentlich nicht aktiviert sein sollten."
        }
      ],
