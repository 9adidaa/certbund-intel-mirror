  "vulnerabilities": [
    {
      "cve": "CVE-2016-4997",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in Linux Kernel im Zusammenhang mit compat_setsockopt(). Ein angemeldeter, lokaler Angreifer kann diese Schwachstelle nutzen, um aus einem Container compat_setsockopt() aufzurufen und dadurch seine Rechte zu erhöhen."
        }
      ],
