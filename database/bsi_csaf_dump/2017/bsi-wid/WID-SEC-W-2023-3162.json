  "vulnerabilities": [
    {
      "cve": "CVE-2017-1383",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in IBM InfoSphere Information Server. Die Schwachstelle tritt auf, wenn XML Daten verarbeitet werden. Ein Angreifer kann dieses zu einem XML External Entity Injection (XXE) Angriff nutzen. In der Folge ist es möglich, vertrauliche Daten einzusehen oder einen Denial of Service herbeizuführen."
        }
      ],
      "product_status": {
        "known_affected": [
