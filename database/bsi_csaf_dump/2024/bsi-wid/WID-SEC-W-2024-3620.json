  "vulnerabilities": [
    {
      "cve": "CVE-2024-10933",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in OpenBSD. Ein Pproblem in der in der readdir-Namensvalidierung ermöglicht unter bestimmten Umständen ein Directory-Traversal. Ein lokaler Angreifer kann diese Schwachstelle ausnutzen, um Informationen offenzulegen. Zur erfolgreichen Ausnutzung ist eine Benutzeraktion erforderlich."
        }
      ],
      "product_status": {
        "known_affected": [
          "T033898",
          "T033194"
        ]
      },
      "release_date": "2024-12-05T23:00:00.000+00:00",
      "title": "CVE-2024-10933"
    },
    {
      "cve": "CVE-2024-11148",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in OpenBSD. Aufgrund einer unsachgemäßen Behandlung von maliziösen fastcgi Anfragen kann es zu einer NULL-Dereferenzierung kommen. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T033194",
          "T030010"
        ]
      },
      "release_date": "2024-12-05T23:00:00.000+00:00",
      "title": "CVE-2024-11148"
    },
    {
      "cve": "CVE-2024-11149",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in OpenBSD. Es besteht ein Problem bei der Wiederherstellung von GDTR-Limits auf Intel-(VMX)-CPUs. Ein lokaler Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T033194"
        ]
      },
      "release_date": "2024-12-05T23:00:00.000+00:00",
