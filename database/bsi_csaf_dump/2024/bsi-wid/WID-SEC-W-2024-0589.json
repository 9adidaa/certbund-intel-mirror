  "vulnerabilities": [
    {
      "cve": "CVE-2023-32969",
      "notes": [
        {
          "category": "description",
          "text": "In QNAP NAS existiert eine Cross-Site Scripting Schwachstelle. HTML und Script-Eingaben werden in Network & Virtual Switch nicht ordnungsgemäß überprüft, bevor sie an den Benutzer zurückgegeben werden. Ein entfernter, anonymer Angreifer kann durch Ausnutzung dieser Schwachstelle beliebigen HTML- und Script-Code durch den Browser des Benutzers im Kontext der betroffenen Seite ausführen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T033350",
          "T033351",
          "T033343",
          "T033341",
          "T033342"
        ]
      },
