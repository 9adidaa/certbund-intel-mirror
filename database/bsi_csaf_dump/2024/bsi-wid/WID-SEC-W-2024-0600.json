  "vulnerabilities": [
    {
      "cve": "CVE-2023-36554",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in Fortinet FortiManager. Dieser Fehler besteht aufgrund einer unsachgemäßen Zugriffskontrolle. Durch das Senden speziell gestalteter Anfragen kann ein entfernter, anonymer Angreifer diese Schwachstelle zur Ausführung von beliebigem Code ausnutzen."
        }
      ],
