  "vulnerabilities": [
    {
      "cve": "CVE-2023-33008",
      "notes": [
        {
          "category": "description",
          "text": "Es existiert eine Schwachstelle in IBM Business Automation Workflow. Diese ist auf einen Fehler bei der Deserialisierung in der Komponente \"BigDecimal\" zurückzuführen. Dies kann durch das Senden einer speziell präperierten JSON-Datei ausgenutzt werden. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um einen Denial of Service Zustand herbeizuführen."
        }
      ],
