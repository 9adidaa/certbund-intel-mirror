  "vulnerabilities": [
    {
      "cve": "CVE-2024-31912",
      "notes": [
        {
          "category": "description",
          "text": "Es besteht eine Schwachstelle in IBM MQ. Dieser Fehler besteht unter bestimmten Konfigurationen aufgrund einer falschen Privilegienvergabe. Ein entfernter authentifizierter Angreifer kann diese Schwachstelle ausnutzen, um seine Privilegien zu erweitern."
        }
      ],
      "product_status": {
        "known_affected": [
          "T021398"
        ]
      },
      "release_date": "2024-06-26T22:00:00.000+00:00",
      "title": "CVE-2024-31912"
    },
    {
      "cve": "CVE-2024-31919",
      "notes": [
        {
          "category": "description",
          "text": "Es bestehen mehrere Schwachstellen in IBM MQ. Diese Schwachstellen bestehen aufgrund eines Fehlers bei der Verarbeitung von Meldungen, wenn ein API-Exit mit MQBUFMH verwendet wird, und eines Fehlers bei der Anwendung von Konfigurationsänderungen. Ein entfernter, anonymer Angreifer kann diese Schwachstellen ausnutzen, um einen Denial-of-Service-Zustand zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T021398"
        ]
      },
      "release_date": "2024-06-26T22:00:00.000+00:00",
      "title": "CVE-2024-31919"
    },
