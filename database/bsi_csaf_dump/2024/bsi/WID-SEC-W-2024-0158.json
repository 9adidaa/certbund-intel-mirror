  "vulnerabilities": [
    {
      "cve": "CVE-2023-49568",
      "notes": [
        {
          "category": "description",
          "text": "In Red Hat Enterprise Linux besteht eine Schwachstelle im Zusammenhang mit dem Advanced Cluster Management für Kubernetes. Der Fehler besteht aufgrund einer Ressourcenerschöpfung in Go-Git-Clients. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, indem er speziell gestaltete Antworten von einem Git-Server bereitstellt, um einen Denial-of-Service-Zustand zu verursachen."
        }
      ],
      "product_status": {
        "known_affected": [
          "T033787",
          "67646",
          "T030999",
          "398363"
        ]
      },
      "release_date": "2024-01-18T23:00:00.000+00:00",
      "title": "CVE-2023-49568"
    },
    {
      "cve": "CVE-2023-49569",
      "notes": [
        {
          "category": "description",
          "text": "In Red Hat Enterprise Linux besteht eine Schwachstelle im Zusammenhang mit dem Advanced Cluster Management für Kubernetes. Der Fehler besteht aufgrund eines Pfad-Traversals in der go-Bibliothek go-git. Ein entfernter, anonymer Angreifer kann diese Schwachstelle ausnutzen, um Dateien im gesamten Dateisystem zu erstellen und zu ändern, wenn Anwendungen das standardmäßige ChrootOS verwenden, was zu Remotecodeausführung führt."
        }
      ],
      "product_status": {
